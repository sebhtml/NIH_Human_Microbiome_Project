if test -f Fetcher/SRS056210/contents/SRR061203_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061203/SRR061203_2.fastq.gz -o Fetcher/SRS056210/wget-logs/SRR061203_2.fastq.gz

