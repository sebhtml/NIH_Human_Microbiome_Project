if test -f Fetcher/SRS017691/contents/SRR059493_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059493/SRR059493_1.fastq.gz -o Fetcher/SRS017691/wget-logs/SRR059493_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017691/checksums/SRR059493_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059493_1.fastq.gz Fetcher/SRS017691/contents
fi

fi
