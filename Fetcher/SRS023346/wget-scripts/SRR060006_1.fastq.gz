if test -f Fetcher/SRS023346/contents/SRR060006_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060006/SRR060006_1.fastq.gz -o Fetcher/SRS023346/wget-logs/SRR060006_1.fastq.gz

if test $(md5sum -c Fetcher/SRS023346/checksums/SRR060006_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060006_1.fastq.gz Fetcher/SRS023346/contents
fi

fi
