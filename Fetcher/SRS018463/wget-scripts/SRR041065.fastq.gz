if test -f Fetcher/SRS018463/contents/SRR041065.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041065/SRR041065.fastq.gz -o Fetcher/SRS018463/wget-logs/SRR041065.fastq.gz

if test $(md5sum -c Fetcher/SRS018463/checksums/SRR041065.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041065.fastq.gz Fetcher/SRS018463/contents
fi

fi
