if test -f Fetcher/SRS015450/contents/SRR062515_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062515/SRR062515_1.fastq.gz -o Fetcher/SRS015450/wget-logs/SRR062515_1.fastq.gz

