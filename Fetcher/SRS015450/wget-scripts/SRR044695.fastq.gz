if test -f Fetcher/SRS015450/contents/SRR044695.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044695/SRR044695.fastq.gz -o Fetcher/SRS015450/wget-logs/SRR044695.fastq.gz

if test $(md5sum -c Fetcher/SRS015450/checksums/SRR044695.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044695.fastq.gz Fetcher/SRS015450/contents
fi

fi
