if test -f Fetcher/SRS024132/contents/SRR059507_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059507/SRR059507_1.fastq.gz -o Fetcher/SRS024132/wget-logs/SRR059507_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024132/checksums/SRR059507_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059507_1.fastq.gz Fetcher/SRS024132/contents
fi

fi
