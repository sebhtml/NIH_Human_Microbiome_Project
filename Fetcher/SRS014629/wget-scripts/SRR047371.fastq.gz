if test -f Fetcher/SRS014629/contents/SRR047371.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047371/SRR047371.fastq.gz -o Fetcher/SRS014629/wget-logs/SRR047371.fastq.gz

if test $(md5sum -c Fetcher/SRS014629/checksums/SRR047371.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047371.fastq.gz Fetcher/SRS014629/contents
fi

fi
