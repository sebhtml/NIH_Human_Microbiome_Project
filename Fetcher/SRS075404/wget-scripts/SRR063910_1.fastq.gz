if test -f Fetcher/SRS075404/contents/SRR063910_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063910/SRR063910_1.fastq.gz -o Fetcher/SRS075404/wget-logs/SRR063910_1.fastq.gz

