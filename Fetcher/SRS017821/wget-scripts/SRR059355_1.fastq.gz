if test -f Fetcher/SRS017821/contents/SRR059355_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059355/SRR059355_1.fastq.gz -o Fetcher/SRS017821/wget-logs/SRR059355_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017821/checksums/SRR059355_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059355_1.fastq.gz Fetcher/SRS017821/contents
fi

fi
