if test -f Fetcher/SRS015440/contents/SRR061354_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061354/SRR061354_1.fastq.gz -o Fetcher/SRS015440/wget-logs/SRR061354_1.fastq.gz

if test $(md5sum -c Fetcher/SRS015440/checksums/SRR061354_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061354_1.fastq.gz Fetcher/SRS015440/contents
fi

fi
