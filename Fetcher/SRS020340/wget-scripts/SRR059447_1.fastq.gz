if test -f Fetcher/SRS020340/contents/SRR059447_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059447/SRR059447_1.fastq.gz -o Fetcher/SRS020340/wget-logs/SRR059447_1.fastq.gz

if test $(md5sum -c Fetcher/SRS020340/checksums/SRR059447_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059447_1.fastq.gz Fetcher/SRS020340/contents
fi

fi
