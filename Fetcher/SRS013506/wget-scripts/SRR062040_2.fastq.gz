if test -f Fetcher/SRS013506/contents/SRR062040_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062040/SRR062040_2.fastq.gz -o Fetcher/SRS013506/wget-logs/SRR062040_2.fastq.gz

if test $(md5sum -c Fetcher/SRS013506/checksums/SRR062040_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062040_2.fastq.gz Fetcher/SRS013506/contents
fi

fi
