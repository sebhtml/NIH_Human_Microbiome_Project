if test -f Fetcher/SRS013506/contents/SRR062039_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062039/SRR062039_2.fastq.gz -o Fetcher/SRS013506/wget-logs/SRR062039_2.fastq.gz

