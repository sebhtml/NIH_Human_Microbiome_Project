if test -f Fetcher/SRS020233/contents/SRR041135.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041135/SRR041135.fastq.gz -o Fetcher/SRS020233/wget-logs/SRR041135.fastq.gz

if test $(md5sum -c Fetcher/SRS020233/checksums/SRR041135.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041135.fastq.gz Fetcher/SRS020233/contents
fi

fi
