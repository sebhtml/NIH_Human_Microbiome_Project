if test -f Fetcher/SRS024620/contents/SRR059925_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059925/SRR059925_1.fastq.gz -o Fetcher/SRS024620/wget-logs/SRR059925_1.fastq.gz

