if test -f Fetcher/SRS056259/contents/SRR050140.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR050/SRR050140/SRR050140.fastq.gz -o Fetcher/SRS056259/wget-logs/SRR050140.fastq.gz

if test $(md5sum -c Fetcher/SRS056259/checksums/SRR050140.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR050140.fastq.gz Fetcher/SRS056259/contents
fi

fi
