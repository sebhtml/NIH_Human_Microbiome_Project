if test -f Fetcher/SRS016018/contents/SRR044251.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044251/SRR044251.fastq.gz -o Fetcher/SRS016018/wget-logs/SRR044251.fastq.gz

if test $(md5sum -c Fetcher/SRS016018/checksums/SRR044251.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044251.fastq.gz Fetcher/SRS016018/contents
fi

fi
