if test -f Fetcher/SRS048536/contents/SRR061732.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061732/SRR061732.fastq.gz -o Fetcher/SRS048536/wget-logs/SRR061732.fastq.gz

if test $(md5sum -c Fetcher/SRS048536/checksums/SRR061732.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061732.fastq.gz Fetcher/SRS048536/contents
fi

fi
