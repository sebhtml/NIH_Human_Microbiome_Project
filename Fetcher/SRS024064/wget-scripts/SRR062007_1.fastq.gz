if test -f Fetcher/SRS024064/contents/SRR062007_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062007/SRR062007_1.fastq.gz -o Fetcher/SRS024064/wget-logs/SRR062007_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024064/checksums/SRR062007_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062007_1.fastq.gz Fetcher/SRS024064/contents
fi

fi
