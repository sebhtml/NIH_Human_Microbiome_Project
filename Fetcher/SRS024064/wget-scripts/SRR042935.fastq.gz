if test -f Fetcher/SRS024064/contents/SRR042935.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042935/SRR042935.fastq.gz -o Fetcher/SRS024064/wget-logs/SRR042935.fastq.gz

if test $(md5sum -c Fetcher/SRS024064/checksums/SRR042935.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042935.fastq.gz Fetcher/SRS024064/contents
fi

fi
