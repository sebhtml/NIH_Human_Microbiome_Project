if test -f Fetcher/SRS019582/contents/SRR047363.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047363/SRR047363.fastq.gz -o Fetcher/SRS019582/wget-logs/SRR047363.fastq.gz

if test $(md5sum -c Fetcher/SRS019582/checksums/SRR047363.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047363.fastq.gz Fetcher/SRS019582/contents
fi

fi
