if test -f Fetcher/SRS017697/contents/SRR059877_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059877/SRR059877_1.fastq.gz -o Fetcher/SRS017697/wget-logs/SRR059877_1.fastq.gz

