if test -f Fetcher/SRS043411/contents/SRR047142.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047142/SRR047142.fastq.gz -o Fetcher/SRS043411/wget-logs/SRR047142.fastq.gz

if test $(md5sum -c Fetcher/SRS043411/checksums/SRR047142.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047142.fastq.gz Fetcher/SRS043411/contents
fi

fi
