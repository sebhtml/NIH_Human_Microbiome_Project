if test -f Fetcher/SRS024561/contents/SRR059409_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059409/SRR059409_1.fastq.gz -o Fetcher/SRS024561/wget-logs/SRR059409_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024561/checksums/SRR059409_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059409_1.fastq.gz Fetcher/SRS024561/contents
fi

fi
