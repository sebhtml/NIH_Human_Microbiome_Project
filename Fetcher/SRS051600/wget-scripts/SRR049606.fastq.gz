if test -f Fetcher/SRS051600/contents/SRR049606.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049606/SRR049606.fastq.gz -o Fetcher/SRS051600/wget-logs/SRR049606.fastq.gz

if test $(md5sum -c Fetcher/SRS051600/checksums/SRR049606.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049606.fastq.gz Fetcher/SRS051600/contents
fi

fi
