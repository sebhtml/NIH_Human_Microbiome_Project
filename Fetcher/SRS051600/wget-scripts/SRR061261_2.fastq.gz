if test -f Fetcher/SRS051600/contents/SRR061261_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061261/SRR061261_2.fastq.gz -o Fetcher/SRS051600/wget-logs/SRR061261_2.fastq.gz

if test $(md5sum -c Fetcher/SRS051600/checksums/SRR061261_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061261_2.fastq.gz Fetcher/SRS051600/contents
fi

fi
