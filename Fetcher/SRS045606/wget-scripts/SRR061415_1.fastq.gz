if test -f Fetcher/SRS045606/contents/SRR061415_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061415/SRR061415_1.fastq.gz -o Fetcher/SRS045606/wget-logs/SRR061415_1.fastq.gz

if test $(md5sum -c Fetcher/SRS045606/checksums/SRR061415_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061415_1.fastq.gz Fetcher/SRS045606/contents
fi

fi
