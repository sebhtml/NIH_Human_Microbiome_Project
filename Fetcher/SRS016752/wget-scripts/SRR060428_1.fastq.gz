if test -f Fetcher/SRS016752/contents/SRR060428_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060428/SRR060428_1.fastq.gz -o Fetcher/SRS016752/wget-logs/SRR060428_1.fastq.gz

if test $(md5sum -c Fetcher/SRS016752/checksums/SRR060428_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060428_1.fastq.gz Fetcher/SRS016752/contents
fi

fi
