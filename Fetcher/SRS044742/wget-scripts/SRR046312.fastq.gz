if test -f Fetcher/SRS044742/contents/SRR046312.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046312/SRR046312.fastq.gz -o Fetcher/SRS044742/wget-logs/SRR046312.fastq.gz

if test $(md5sum -c Fetcher/SRS044742/checksums/SRR046312.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046312.fastq.gz Fetcher/SRS044742/contents
fi

fi
