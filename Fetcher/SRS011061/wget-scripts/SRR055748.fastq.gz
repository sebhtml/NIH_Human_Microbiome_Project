if test -f Fetcher/SRS011061/contents/SRR055748.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR055/SRR055748/SRR055748.fastq.gz -o Fetcher/SRS011061/wget-logs/SRR055748.fastq.gz

if test $(md5sum -c Fetcher/SRS011061/checksums/SRR055748.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR055748.fastq.gz Fetcher/SRS011061/contents
fi

fi
