if test -f Fetcher/SRS015663/contents/SRR044161.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044161/SRR044161.fastq.gz -o Fetcher/SRS015663/wget-logs/SRR044161.fastq.gz

if test $(md5sum -c Fetcher/SRS015663/checksums/SRR044161.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044161.fastq.gz Fetcher/SRS015663/contents
fi

fi
