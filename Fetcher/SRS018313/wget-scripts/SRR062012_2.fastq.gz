if test -f Fetcher/SRS018313/contents/SRR062012_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062012/SRR062012_2.fastq.gz -o Fetcher/SRS018313/wget-logs/SRR062012_2.fastq.gz

if test $(md5sum -c Fetcher/SRS018313/checksums/SRR062012_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062012_2.fastq.gz Fetcher/SRS018313/contents
fi

fi
