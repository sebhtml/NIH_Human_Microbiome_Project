if test -f Fetcher/SRS018313/contents/SRR041495.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041495/SRR041495.fastq.gz -o Fetcher/SRS018313/wget-logs/SRR041495.fastq.gz

if test $(md5sum -c Fetcher/SRS018313/checksums/SRR041495.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041495.fastq.gz Fetcher/SRS018313/contents
fi

fi
