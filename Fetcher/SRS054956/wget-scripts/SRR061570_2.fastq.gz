if test -f Fetcher/SRS054956/contents/SRR061570_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061570/SRR061570_2.fastq.gz -o Fetcher/SRS054956/wget-logs/SRR061570_2.fastq.gz

