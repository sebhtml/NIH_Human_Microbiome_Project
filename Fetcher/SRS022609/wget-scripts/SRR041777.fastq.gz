if test -f Fetcher/SRS022609/contents/SRR041777.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041777/SRR041777.fastq.gz -o Fetcher/SRS022609/wget-logs/SRR041777.fastq.gz

if test $(md5sum -c Fetcher/SRS022609/checksums/SRR041777.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041777.fastq.gz Fetcher/SRS022609/contents
fi

fi
