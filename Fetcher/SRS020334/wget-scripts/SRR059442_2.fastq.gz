if test -f Fetcher/SRS020334/contents/SRR059442_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059442/SRR059442_2.fastq.gz -o Fetcher/SRS020334/wget-logs/SRR059442_2.fastq.gz

if test $(md5sum -c Fetcher/SRS020334/checksums/SRR059442_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059442_2.fastq.gz Fetcher/SRS020334/contents
fi

fi
