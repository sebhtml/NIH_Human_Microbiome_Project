if test -f Fetcher/SRS020336/contents/SRR059444_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059444/SRR059444_1.fastq.gz -o Fetcher/SRS020336/wget-logs/SRR059444_1.fastq.gz

if test $(md5sum -c Fetcher/SRS020336/checksums/SRR059444_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059444_1.fastq.gz Fetcher/SRS020336/contents
fi

fi
