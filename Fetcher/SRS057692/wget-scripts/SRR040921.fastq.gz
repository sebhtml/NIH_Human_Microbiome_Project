if test -f Fetcher/SRS057692/contents/SRR040921.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040921/SRR040921.fastq.gz -o Fetcher/SRS057692/wget-logs/SRR040921.fastq.gz

if test $(md5sum -c Fetcher/SRS057692/checksums/SRR040921.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040921.fastq.gz Fetcher/SRS057692/contents
fi

fi
