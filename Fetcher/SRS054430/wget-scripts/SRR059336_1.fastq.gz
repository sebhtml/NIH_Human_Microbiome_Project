if test -f Fetcher/SRS054430/contents/SRR059336_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059336/SRR059336_1.fastq.gz -o Fetcher/SRS054430/wget-logs/SRR059336_1.fastq.gz

