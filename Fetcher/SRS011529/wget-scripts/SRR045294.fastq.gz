if test -f Fetcher/SRS011529/contents/SRR045294.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR045/SRR045294/SRR045294.fastq.gz -o Fetcher/SRS011529/wget-logs/SRR045294.fastq.gz

if test $(md5sum -c Fetcher/SRS011529/checksums/SRR045294.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR045294.fastq.gz Fetcher/SRS011529/contents
fi

fi
