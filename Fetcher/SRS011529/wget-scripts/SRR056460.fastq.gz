if test -f Fetcher/SRS011529/contents/SRR056460.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR056/SRR056460/SRR056460.fastq.gz -o Fetcher/SRS011529/wget-logs/SRR056460.fastq.gz

if test $(md5sum -c Fetcher/SRS011529/checksums/SRR056460.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR056460.fastq.gz Fetcher/SRS011529/contents
fi

fi
