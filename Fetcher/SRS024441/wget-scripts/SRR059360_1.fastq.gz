if test -f Fetcher/SRS024441/contents/SRR059360_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059360/SRR059360_1.fastq.gz -o Fetcher/SRS024441/wget-logs/SRR059360_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024441/checksums/SRR059360_1.fastq.gz|grep OK$|wc -l) = 1
