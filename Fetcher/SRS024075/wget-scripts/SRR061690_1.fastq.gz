if test -f Fetcher/SRS024075/contents/SRR061690_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061690/SRR061690_1.fastq.gz -o Fetcher/SRS024075/wget-logs/SRR061690_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024075/checksums/SRR061690_1.fastq.gz|grep OK$|wc -l) = 1
