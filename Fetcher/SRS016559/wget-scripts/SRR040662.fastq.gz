if test -f Fetcher/SRS016559/contents/SRR040662.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040662/SRR040662.fastq.gz -o Fetcher/SRS016559/wget-logs/SRR040662.fastq.gz

if test $(md5sum -c Fetcher/SRS016559/checksums/SRR040662.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040662.fastq.gz Fetcher/SRS016559/contents
fi

fi
