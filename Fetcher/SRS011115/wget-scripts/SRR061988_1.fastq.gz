if test -f Fetcher/SRS011115/contents/SRR061988_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061988/SRR061988_1.fastq.gz -o Fetcher/SRS011115/wget-logs/SRR061988_1.fastq.gz

if test $(md5sum -c Fetcher/SRS011115/checksums/SRR061988_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061988_1.fastq.gz Fetcher/SRS011115/contents
fi

fi
