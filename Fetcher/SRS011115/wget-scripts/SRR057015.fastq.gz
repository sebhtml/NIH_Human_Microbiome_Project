if test -f Fetcher/SRS011115/contents/SRR057015.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057015/SRR057015.fastq.gz -o Fetcher/SRS011115/wget-logs/SRR057015.fastq.gz

if test $(md5sum -c Fetcher/SRS011115/checksums/SRR057015.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057015.fastq.gz Fetcher/SRS011115/contents
fi

fi
