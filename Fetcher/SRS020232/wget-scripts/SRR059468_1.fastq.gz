if test -f Fetcher/SRS020232/contents/SRR059468_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059468/SRR059468_1.fastq.gz -o Fetcher/SRS020232/wget-logs/SRR059468_1.fastq.gz

