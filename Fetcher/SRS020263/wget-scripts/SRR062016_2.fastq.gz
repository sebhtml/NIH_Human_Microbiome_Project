if test -f Fetcher/SRS020263/contents/SRR062016_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062016/SRR062016_2.fastq.gz -o Fetcher/SRS020263/wget-logs/SRR062016_2.fastq.gz

if test $(md5sum -c Fetcher/SRS020263/checksums/SRR062016_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062016_2.fastq.gz Fetcher/SRS020263/contents
fi

fi
