if test -f Fetcher/SRS011247/contents/SRR056987.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR056/SRR056987/SRR056987.fastq.gz -o Fetcher/SRS011247/wget-logs/SRR056987.fastq.gz

if test $(md5sum -c Fetcher/SRS011247/checksums/SRR056987.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR056987.fastq.gz Fetcher/SRS011247/contents
fi

fi
