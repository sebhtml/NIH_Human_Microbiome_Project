if test -f Fetcher/SRS011247/contents/SRR055767.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR055/SRR055767/SRR055767.fastq.gz -o Fetcher/SRS011247/wget-logs/SRR055767.fastq.gz

if test $(md5sum -c Fetcher/SRS011247/checksums/SRR055767.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR055767.fastq.gz Fetcher/SRS011247/contents
fi

fi
