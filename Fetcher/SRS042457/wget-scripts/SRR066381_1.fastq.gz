if test -f Fetcher/SRS042457/contents/SRR066381_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR066/SRR066381/SRR066381_1.fastq.gz -o Fetcher/SRS042457/wget-logs/SRR066381_1.fastq.gz

