if test -f Fetcher/SRS013637/contents/SRR059503_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059503/SRR059503_2.fastq.gz -o Fetcher/SRS013637/wget-logs/SRR059503_2.fastq.gz

if test $(md5sum -c Fetcher/SRS013637/checksums/SRR059503_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059503_2.fastq.gz Fetcher/SRS013637/contents
fi

fi
