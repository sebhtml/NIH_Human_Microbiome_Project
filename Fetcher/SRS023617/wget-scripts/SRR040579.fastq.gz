if test -f Fetcher/SRS023617/contents/SRR040579.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040579/SRR040579.fastq.gz -o Fetcher/SRS023617/wget-logs/SRR040579.fastq.gz

if test $(md5sum -c Fetcher/SRS023617/checksums/SRR040579.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040579.fastq.gz Fetcher/SRS023617/contents
fi

fi
