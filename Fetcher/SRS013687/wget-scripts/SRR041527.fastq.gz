if test -f Fetcher/SRS013687/contents/SRR041527.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041527/SRR041527.fastq.gz -o Fetcher/SRS013687/wget-logs/SRR041527.fastq.gz

if test $(md5sum -c Fetcher/SRS013687/checksums/SRR041527.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041527.fastq.gz Fetcher/SRS013687/contents
fi

fi
