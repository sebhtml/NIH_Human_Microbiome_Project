if test -f Fetcher/SRS047225/contents/SRR061943_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061943/SRR061943_2.fastq.gz -o Fetcher/SRS047225/wget-logs/SRR061943_2.fastq.gz

