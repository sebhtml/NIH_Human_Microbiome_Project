if test -f Fetcher/SRS047225/contents/SRR042808.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042808/SRR042808.fastq.gz -o Fetcher/SRS047225/wget-logs/SRR042808.fastq.gz

if test $(md5sum -c Fetcher/SRS047225/checksums/SRR042808.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042808.fastq.gz Fetcher/SRS047225/contents
fi

fi
