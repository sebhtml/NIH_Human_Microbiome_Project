if test -f Fetcher/SRS011355/contents/SRR057244.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057244/SRR057244.fastq.gz -o Fetcher/SRS011355/wget-logs/SRR057244.fastq.gz

if test $(md5sum -c Fetcher/SRS011355/checksums/SRR057244.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057244.fastq.gz Fetcher/SRS011355/contents
fi

fi
