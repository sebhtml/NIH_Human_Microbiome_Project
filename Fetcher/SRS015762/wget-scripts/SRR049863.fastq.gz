if test -f Fetcher/SRS015762/contents/SRR049863.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049863/SRR049863.fastq.gz -o Fetcher/SRS015762/wget-logs/SRR049863.fastq.gz

if test $(md5sum -c Fetcher/SRS015762/checksums/SRR049863.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049863.fastq.gz Fetcher/SRS015762/contents
fi

fi
