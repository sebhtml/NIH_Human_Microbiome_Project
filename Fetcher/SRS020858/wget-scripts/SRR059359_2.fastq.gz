if test -f Fetcher/SRS020858/contents/SRR059359_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059359/SRR059359_2.fastq.gz -o Fetcher/SRS020858/wget-logs/SRR059359_2.fastq.gz

if test $(md5sum -c Fetcher/SRS020858/checksums/SRR059359_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059359_2.fastq.gz Fetcher/SRS020858/contents
fi

fi
