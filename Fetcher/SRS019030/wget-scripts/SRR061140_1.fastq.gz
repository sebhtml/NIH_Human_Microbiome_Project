if test -f Fetcher/SRS019030/contents/SRR061140_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061140/SRR061140_1.fastq.gz -o Fetcher/SRS019030/wget-logs/SRR061140_1.fastq.gz

