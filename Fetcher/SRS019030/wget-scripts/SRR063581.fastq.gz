if test -f Fetcher/SRS019030/contents/SRR063581.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063581/SRR063581.fastq.gz -o Fetcher/SRS019030/wget-logs/SRR063581.fastq.gz

if test $(md5sum -c Fetcher/SRS019030/checksums/SRR063581.fastq.gz|grep OK$|wc -l) = 1
