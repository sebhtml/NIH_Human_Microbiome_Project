if test -f Fetcher/SRS015890/contents/SRR042181_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042181/SRR042181_1.fastq.gz -o Fetcher/SRS015890/wget-logs/SRR042181_1.fastq.gz

if test $(md5sum -c Fetcher/SRS015890/checksums/SRR042181_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042181_1.fastq.gz Fetcher/SRS015890/contents
