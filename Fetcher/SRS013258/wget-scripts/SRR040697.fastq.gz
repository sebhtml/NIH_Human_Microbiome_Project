if test -f Fetcher/SRS013258/contents/SRR040697.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040697/SRR040697.fastq.gz -o Fetcher/SRS013258/wget-logs/SRR040697.fastq.gz

if test $(md5sum -c Fetcher/SRS013258/checksums/SRR040697.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040697.fastq.gz Fetcher/SRS013258/contents
fi

fi
