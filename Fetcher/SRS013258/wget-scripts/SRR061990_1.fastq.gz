if test -f Fetcher/SRS013258/contents/SRR061990_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061990/SRR061990_1.fastq.gz -o Fetcher/SRS013258/wget-logs/SRR061990_1.fastq.gz

if test $(md5sum -c Fetcher/SRS013258/checksums/SRR061990_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061990_1.fastq.gz Fetcher/SRS013258/contents
fi

fi
