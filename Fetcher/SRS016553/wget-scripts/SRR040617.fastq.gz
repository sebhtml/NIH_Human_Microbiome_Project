if test -f Fetcher/SRS016553/contents/SRR040617.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040617/SRR040617.fastq.gz -o Fetcher/SRS016553/wget-logs/SRR040617.fastq.gz

if test $(md5sum -c Fetcher/SRS016553/checksums/SRR040617.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040617.fastq.gz Fetcher/SRS016553/contents
fi

fi
