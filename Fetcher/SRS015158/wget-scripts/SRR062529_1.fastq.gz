if test -f Fetcher/SRS015158/contents/SRR062529_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062529/SRR062529_1.fastq.gz -o Fetcher/SRS015158/wget-logs/SRR062529_1.fastq.gz

