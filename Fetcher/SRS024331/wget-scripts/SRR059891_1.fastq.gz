if test -f Fetcher/SRS024331/contents/SRR059891_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059891/SRR059891_1.fastq.gz -o Fetcher/SRS024331/wget-logs/SRR059891_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024331/checksums/SRR059891_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059891_1.fastq.gz Fetcher/SRS024331/contents
fi

fi
