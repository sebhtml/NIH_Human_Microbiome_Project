if test -f Fetcher/SRS058182/contents/SRR040988.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040988/SRR040988.fastq.gz -o Fetcher/SRS058182/wget-logs/SRR040988.fastq.gz

if test $(md5sum -c Fetcher/SRS058182/checksums/SRR040988.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040988.fastq.gz Fetcher/SRS058182/contents
fi

fi
