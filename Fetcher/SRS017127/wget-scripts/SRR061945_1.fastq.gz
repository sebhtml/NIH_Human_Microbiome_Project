if test -f Fetcher/SRS017127/contents/SRR061945_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061945/SRR061945_1.fastq.gz -o Fetcher/SRS017127/wget-logs/SRR061945_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017127/checksums/SRR061945_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061945_1.fastq.gz Fetcher/SRS017127/contents
fi

fi
