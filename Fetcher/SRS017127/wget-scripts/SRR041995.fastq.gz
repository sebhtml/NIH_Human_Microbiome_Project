if test -f Fetcher/SRS017127/contents/SRR041995.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041995/SRR041995.fastq.gz -o Fetcher/SRS017127/wget-logs/SRR041995.fastq.gz

if test $(md5sum -c Fetcher/SRS017127/checksums/SRR041995.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041995.fastq.gz Fetcher/SRS017127/contents
fi

fi
