if test -f Fetcher/SRS019607/contents/SRR061213_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061213/SRR061213_1.fastq.gz -o Fetcher/SRS019607/wget-logs/SRR061213_1.fastq.gz

