if test -f Fetcher/SRS053335/contents/SRR049343.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049343/SRR049343.fastq.gz -o Fetcher/SRS053335/wget-logs/SRR049343.fastq.gz

if test $(md5sum -c Fetcher/SRS053335/checksums/SRR049343.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049343.fastq.gz Fetcher/SRS053335/contents
fi

fi
