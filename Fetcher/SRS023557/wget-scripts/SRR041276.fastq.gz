if test -f Fetcher/SRS023557/contents/SRR041276.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041276/SRR041276.fastq.gz -o Fetcher/SRS023557/wget-logs/SRR041276.fastq.gz

if test $(md5sum -c Fetcher/SRS023557/checksums/SRR041276.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041276.fastq.gz Fetcher/SRS023557/contents
fi

fi
