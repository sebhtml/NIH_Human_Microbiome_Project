if test -f Fetcher/SRS016331/contents/SRR044302.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044302/SRR044302.fastq.gz -o Fetcher/SRS016331/wget-logs/SRR044302.fastq.gz

if test $(md5sum -c Fetcher/SRS016331/checksums/SRR044302.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044302.fastq.gz Fetcher/SRS016331/contents
fi

fi
