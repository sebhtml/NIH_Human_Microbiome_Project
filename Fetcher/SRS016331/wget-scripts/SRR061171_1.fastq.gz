	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061171/SRR061171_1.fastq.gz -o Fetcher/SRS016331/wget-logs/SRR061171_1.fastq.gz

