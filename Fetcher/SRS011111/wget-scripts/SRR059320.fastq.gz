if test -f Fetcher/SRS011111/contents/SRR059320.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059320/SRR059320.fastq.gz -o Fetcher/SRS011111/wget-logs/SRR059320.fastq.gz

if test $(md5sum -c Fetcher/SRS011111/checksums/SRR059320.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059320.fastq.gz Fetcher/SRS011111/contents
fi

fi
