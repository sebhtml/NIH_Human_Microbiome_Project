if test -f Fetcher/SRS011111/contents/SRR055681.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR055/SRR055681/SRR055681.fastq.gz -o Fetcher/SRS011111/wget-logs/SRR055681.fastq.gz

if test $(md5sum -c Fetcher/SRS011111/checksums/SRR055681.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR055681.fastq.gz Fetcher/SRS011111/contents
fi

fi
