if test -f Fetcher/SRS011111/contents/SRR062045_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062045/SRR062045_2.fastq.gz -o Fetcher/SRS011111/wget-logs/SRR062045_2.fastq.gz

if test $(md5sum -c Fetcher/SRS011111/checksums/SRR062045_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062045_2.fastq.gz Fetcher/SRS011111/contents
fi

fi
