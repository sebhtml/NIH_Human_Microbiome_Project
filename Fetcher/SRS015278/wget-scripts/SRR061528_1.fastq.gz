if test -f Fetcher/SRS015278/contents/SRR061528_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061528/SRR061528_1.fastq.gz -o Fetcher/SRS015278/wget-logs/SRR061528_1.fastq.gz

