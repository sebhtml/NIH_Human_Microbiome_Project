if test -f Fetcher/SRS064809/contents/SRR049516.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049516/SRR049516.fastq.gz -o Fetcher/SRS064809/wget-logs/SRR049516.fastq.gz

if test $(md5sum -c Fetcher/SRS064809/checksums/SRR049516.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049516.fastq.gz Fetcher/SRS064809/contents
fi

fi
