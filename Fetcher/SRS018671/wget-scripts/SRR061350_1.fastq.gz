if test -f Fetcher/SRS018671/contents/SRR061350_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061350/SRR061350_1.fastq.gz -o Fetcher/SRS018671/wget-logs/SRR061350_1.fastq.gz

if test $(md5sum -c Fetcher/SRS018671/checksums/SRR061350_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061350_1.fastq.gz Fetcher/SRS018671/contents
fi

fi
