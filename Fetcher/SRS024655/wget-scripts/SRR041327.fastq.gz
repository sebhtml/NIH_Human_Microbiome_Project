if test -f Fetcher/SRS024655/contents/SRR041327.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041327/SRR041327.fastq.gz -o Fetcher/SRS024655/wget-logs/SRR041327.fastq.gz

if test $(md5sum -c Fetcher/SRS024655/checksums/SRR041327.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041327.fastq.gz Fetcher/SRS024655/contents
fi

fi
