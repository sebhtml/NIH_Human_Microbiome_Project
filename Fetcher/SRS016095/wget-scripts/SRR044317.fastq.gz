if test -f Fetcher/SRS016095/contents/SRR044317.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044317/SRR044317.fastq.gz -o Fetcher/SRS016095/wget-logs/SRR044317.fastq.gz

if test $(md5sum -c Fetcher/SRS016095/checksums/SRR044317.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044317.fastq.gz Fetcher/SRS016095/contents
fi

fi
