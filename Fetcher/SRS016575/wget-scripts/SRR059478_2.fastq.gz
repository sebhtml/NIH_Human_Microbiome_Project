if test -f Fetcher/SRS016575/contents/SRR059478_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059478/SRR059478_2.fastq.gz -o Fetcher/SRS016575/wget-logs/SRR059478_2.fastq.gz

if test $(md5sum -c Fetcher/SRS016575/checksums/SRR059478_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059478_2.fastq.gz Fetcher/SRS016575/contents
fi

fi
