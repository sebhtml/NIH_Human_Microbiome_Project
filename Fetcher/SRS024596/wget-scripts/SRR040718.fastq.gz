if test -f Fetcher/SRS024596/contents/SRR040718.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040718/SRR040718.fastq.gz -o Fetcher/SRS024596/wget-logs/SRR040718.fastq.gz

if test $(md5sum -c Fetcher/SRS024596/checksums/SRR040718.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040718.fastq.gz Fetcher/SRS024596/contents
fi

fi
