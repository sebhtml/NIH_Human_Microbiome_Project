if test -f Fetcher/SRS024596/contents/SRR062018_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062018/SRR062018_2.fastq.gz -o Fetcher/SRS024596/wget-logs/SRR062018_2.fastq.gz

if test $(md5sum -c Fetcher/SRS024596/checksums/SRR062018_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062018_2.fastq.gz Fetcher/SRS024596/contents
fi

fi
