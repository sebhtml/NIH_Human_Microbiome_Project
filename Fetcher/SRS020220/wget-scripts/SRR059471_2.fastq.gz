if test -f Fetcher/SRS020220/contents/SRR059471_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059471/SRR059471_2.fastq.gz -o Fetcher/SRS020220/wget-logs/SRR059471_2.fastq.gz

if test $(md5sum -c Fetcher/SRS020220/checksums/SRR059471_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059471_2.fastq.gz Fetcher/SRS020220/contents
fi

fi
