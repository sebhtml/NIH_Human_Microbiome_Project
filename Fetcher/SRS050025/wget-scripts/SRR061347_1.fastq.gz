if test -f Fetcher/SRS050025/contents/SRR061347_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061347/SRR061347_1.fastq.gz -o Fetcher/SRS050025/wget-logs/SRR061347_1.fastq.gz

if test $(md5sum -c Fetcher/SRS050025/checksums/SRR061347_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061347_1.fastq.gz Fetcher/SRS050025/contents
