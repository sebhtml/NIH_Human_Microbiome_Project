if test -f Fetcher/SRS015051/contents/SRR353621_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR353/SRR353621/SRR353621_1.fastq.gz -o Fetcher/SRS015051/wget-logs/SRR353621_1.fastq.gz

