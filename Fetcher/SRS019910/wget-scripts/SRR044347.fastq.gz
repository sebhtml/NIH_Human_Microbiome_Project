if test -f Fetcher/SRS019910/contents/SRR044347.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044347/SRR044347.fastq.gz -o Fetcher/SRS019910/wget-logs/SRR044347.fastq.gz

if test $(md5sum -c Fetcher/SRS019910/checksums/SRR044347.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044347.fastq.gz Fetcher/SRS019910/contents
fi

fi
