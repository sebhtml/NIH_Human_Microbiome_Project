if test -f Fetcher/SRS015369/contents/SRR063570.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063570/SRR063570.fastq.gz -o Fetcher/SRS015369/wget-logs/SRR063570.fastq.gz

if test $(md5sum -c Fetcher/SRS015369/checksums/SRR063570.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR063570.fastq.gz Fetcher/SRS015369/contents
fi

fi
