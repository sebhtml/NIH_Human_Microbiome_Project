if test -f Fetcher/SRS065431/contents/SRR060002_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060002/SRR060002_1.fastq.gz -o Fetcher/SRS065431/wget-logs/SRR060002_1.fastq.gz

if test $(md5sum -c Fetcher/SRS065431/checksums/SRR060002_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060002_1.fastq.gz Fetcher/SRS065431/contents
fi

fi
