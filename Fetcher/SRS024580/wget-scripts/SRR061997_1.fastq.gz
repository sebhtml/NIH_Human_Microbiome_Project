if test -f Fetcher/SRS024580/contents/SRR061997_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061997/SRR061997_1.fastq.gz -o Fetcher/SRS024580/wget-logs/SRR061997_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024580/checksums/SRR061997_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061997_1.fastq.gz Fetcher/SRS024580/contents
fi

fi
