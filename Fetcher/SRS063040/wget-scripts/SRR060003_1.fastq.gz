if test -f Fetcher/SRS063040/contents/SRR060003_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060003/SRR060003_1.fastq.gz -o Fetcher/SRS063040/wget-logs/SRR060003_1.fastq.gz

if test $(md5sum -c Fetcher/SRS063040/checksums/SRR060003_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060003_1.fastq.gz Fetcher/SRS063040/contents
fi

fi
