if test -f Fetcher/SRS063040/contents/SRR063907.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063907/SRR063907.fastq.gz -o Fetcher/SRS063040/wget-logs/SRR063907.fastq.gz

if test $(md5sum -c Fetcher/SRS063040/checksums/SRR063907.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR063907.fastq.gz Fetcher/SRS063040/contents
fi

fi
