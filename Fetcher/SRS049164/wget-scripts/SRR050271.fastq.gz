if test -f Fetcher/SRS049164/contents/SRR050271.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR050/SRR050271/SRR050271.fastq.gz -o Fetcher/SRS049164/wget-logs/SRR050271.fastq.gz

if test $(md5sum -c Fetcher/SRS049164/checksums/SRR050271.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR050271.fastq.gz Fetcher/SRS049164/contents
fi

fi
