if test -f Fetcher/SRS018443/contents/SRR042007.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042007/SRR042007.fastq.gz -o Fetcher/SRS018443/wget-logs/SRR042007.fastq.gz

if test $(md5sum -c Fetcher/SRS018443/checksums/SRR042007.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042007.fastq.gz Fetcher/SRS018443/contents
fi

fi
