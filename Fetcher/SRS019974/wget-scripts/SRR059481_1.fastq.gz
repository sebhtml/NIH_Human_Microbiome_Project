if test -f Fetcher/SRS019974/contents/SRR059481_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059481/SRR059481_1.fastq.gz -o Fetcher/SRS019974/wget-logs/SRR059481_1.fastq.gz

