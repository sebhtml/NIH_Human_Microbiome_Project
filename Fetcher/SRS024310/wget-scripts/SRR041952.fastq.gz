if test -f Fetcher/SRS024310/contents/SRR041952.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041952/SRR041952.fastq.gz -o Fetcher/SRS024310/wget-logs/SRR041952.fastq.gz

if test $(md5sum -c Fetcher/SRS024310/checksums/SRR041952.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041952.fastq.gz Fetcher/SRS024310/contents
fi

fi
