if test -f Fetcher/SRS016292/contents/SRR061525_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061525/SRR061525_1.fastq.gz -o Fetcher/SRS016292/wget-logs/SRR061525_1.fastq.gz

if test $(md5sum -c Fetcher/SRS016292/checksums/SRR061525_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061525_1.fastq.gz Fetcher/SRS016292/contents
fi

fi
