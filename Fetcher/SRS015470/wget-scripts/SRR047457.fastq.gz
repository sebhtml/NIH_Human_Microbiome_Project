if test -f Fetcher/SRS015470/contents/SRR047457.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047457/SRR047457.fastq.gz -o Fetcher/SRS015470/wget-logs/SRR047457.fastq.gz

if test $(md5sum -c Fetcher/SRS015470/checksums/SRR047457.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047457.fastq.gz Fetcher/SRS015470/contents
fi

fi
