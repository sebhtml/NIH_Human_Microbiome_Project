if test -f Fetcher/SRS021484/contents/SRR041529.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041529/SRR041529.fastq.gz -o Fetcher/SRS021484/wget-logs/SRR041529.fastq.gz

if test $(md5sum -c Fetcher/SRS021484/checksums/SRR041529.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041529.fastq.gz Fetcher/SRS021484/contents
fi

fi
