if test -f Fetcher/SRS012285/contents/SRR059451_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059451/SRR059451_1.fastq.gz -o Fetcher/SRS012285/wget-logs/SRR059451_1.fastq.gz

if test $(md5sum -c Fetcher/SRS012285/checksums/SRR059451_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059451_1.fastq.gz Fetcher/SRS012285/contents
fi

fi
