if test -f Fetcher/SRS016753/contents/SRR059830_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059830/SRR059830_1.fastq.gz -o Fetcher/SRS016753/wget-logs/SRR059830_1.fastq.gz

if test $(md5sum -c Fetcher/SRS016753/checksums/SRR059830_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059830_1.fastq.gz Fetcher/SRS016753/contents
fi

fi
