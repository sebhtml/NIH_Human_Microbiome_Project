if test -f Fetcher/SRS023938/contents/SRR041514.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041514/SRR041514.fastq.gz -o Fetcher/SRS023938/wget-logs/SRR041514.fastq.gz

if test $(md5sum -c Fetcher/SRS023938/checksums/SRR041514.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041514.fastq.gz Fetcher/SRS023938/contents
fi

fi
