if test -f Fetcher/SRS014271/contents/SRR045624.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR045/SRR045624/SRR045624.fastq.gz -o Fetcher/SRS014271/wget-logs/SRR045624.fastq.gz

if test $(md5sum -c Fetcher/SRS014271/checksums/SRR045624.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR045624.fastq.gz Fetcher/SRS014271/contents
fi

fi
