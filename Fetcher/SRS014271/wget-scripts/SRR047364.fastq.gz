if test -f Fetcher/SRS014271/contents/SRR047364.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047364/SRR047364.fastq.gz -o Fetcher/SRS014271/wget-logs/SRR047364.fastq.gz

if test $(md5sum -c Fetcher/SRS014271/checksums/SRR047364.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047364.fastq.gz Fetcher/SRS014271/contents
fi

fi
