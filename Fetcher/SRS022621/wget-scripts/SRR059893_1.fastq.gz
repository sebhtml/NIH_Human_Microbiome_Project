if test -f Fetcher/SRS022621/contents/SRR059893_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059893/SRR059893_1.fastq.gz -o Fetcher/SRS022621/wget-logs/SRR059893_1.fastq.gz

