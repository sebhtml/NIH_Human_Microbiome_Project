if test -f Fetcher/SRS019685/contents/SRR050230.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR050/SRR050230/SRR050230.fastq.gz -o Fetcher/SRS019685/wget-logs/SRR050230.fastq.gz

if test $(md5sum -c Fetcher/SRS019685/checksums/SRR050230.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR050230.fastq.gz Fetcher/SRS019685/contents
fi

fi
