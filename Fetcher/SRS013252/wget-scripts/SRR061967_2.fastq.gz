if test -f Fetcher/SRS013252/contents/SRR061967_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061967/SRR061967_2.fastq.gz -o Fetcher/SRS013252/wget-logs/SRR061967_2.fastq.gz

