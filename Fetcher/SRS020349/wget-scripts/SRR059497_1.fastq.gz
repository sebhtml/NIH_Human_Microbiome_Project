if test -f Fetcher/SRS020349/contents/SRR059497_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059497/SRR059497_1.fastq.gz -o Fetcher/SRS020349/wget-logs/SRR059497_1.fastq.gz

if test $(md5sum -c Fetcher/SRS020349/checksums/SRR059497_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059497_1.fastq.gz Fetcher/SRS020349/contents
fi

fi
