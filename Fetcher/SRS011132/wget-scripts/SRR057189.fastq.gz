if test -f Fetcher/SRS011132/contents/SRR057189.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057189/SRR057189.fastq.gz -o Fetcher/SRS011132/wget-logs/SRR057189.fastq.gz

if test $(md5sum -c Fetcher/SRS011132/checksums/SRR057189.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057189.fastq.gz Fetcher/SRS011132/contents
fi

fi
