if test -f Fetcher/SRS022071/contents/SRR059454_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059454/SRR059454_2.fastq.gz -o Fetcher/SRS022071/wget-logs/SRR059454_2.fastq.gz

if test $(md5sum -c Fetcher/SRS022071/checksums/SRR059454_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059454_2.fastq.gz Fetcher/SRS022071/contents
fi

fi
