if test -f Fetcher/SRS016944/contents/SRR042838.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042838/SRR042838.fastq.gz -o Fetcher/SRS016944/wget-logs/SRR042838.fastq.gz

if test $(md5sum -c Fetcher/SRS016944/checksums/SRR042838.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042838.fastq.gz Fetcher/SRS016944/contents
fi

fi
