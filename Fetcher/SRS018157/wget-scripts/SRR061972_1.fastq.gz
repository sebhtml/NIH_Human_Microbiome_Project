if test -f Fetcher/SRS018157/contents/SRR061972_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061972/SRR061972_1.fastq.gz -o Fetcher/SRS018157/wget-logs/SRR061972_1.fastq.gz

