if test -f Fetcher/SRS015431/contents/SRR061132_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061132/SRR061132_1.fastq.gz -o Fetcher/SRS015431/wget-logs/SRR061132_1.fastq.gz

