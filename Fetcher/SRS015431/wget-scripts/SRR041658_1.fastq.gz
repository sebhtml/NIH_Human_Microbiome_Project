if test -f Fetcher/SRS015431/contents/SRR041658_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041658/SRR041658_1.fastq.gz -o Fetcher/SRS015431/wget-logs/SRR041658_1.fastq.gz

