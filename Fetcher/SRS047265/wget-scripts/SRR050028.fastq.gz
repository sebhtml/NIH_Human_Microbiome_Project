if test -f Fetcher/SRS047265/contents/SRR050028.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR050/SRR050028/SRR050028.fastq.gz -o Fetcher/SRS047265/wget-logs/SRR050028.fastq.gz

if test $(md5sum -c Fetcher/SRS047265/checksums/SRR050028.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR050028.fastq.gz Fetcher/SRS047265/contents
fi

fi
