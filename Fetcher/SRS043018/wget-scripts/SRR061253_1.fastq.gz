if test -f Fetcher/SRS043018/contents/SRR061253_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061253/SRR061253_1.fastq.gz -o Fetcher/SRS043018/wget-logs/SRR061253_1.fastq.gz

