if test -f Fetcher/SRS016002/contents/SRR044181.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044181/SRR044181.fastq.gz -o Fetcher/SRS016002/wget-logs/SRR044181.fastq.gz

if test $(md5sum -c Fetcher/SRS016002/checksums/SRR044181.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044181.fastq.gz Fetcher/SRS016002/contents
fi

fi
