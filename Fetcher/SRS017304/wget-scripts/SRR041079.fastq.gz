if test -f Fetcher/SRS017304/contents/SRR041079.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041079/SRR041079.fastq.gz -o Fetcher/SRS017304/wget-logs/SRR041079.fastq.gz

if test $(md5sum -c Fetcher/SRS017304/checksums/SRR041079.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041079.fastq.gz Fetcher/SRS017304/contents
fi

fi
