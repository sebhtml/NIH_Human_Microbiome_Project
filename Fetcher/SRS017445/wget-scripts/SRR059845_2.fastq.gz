if test -f Fetcher/SRS017445/contents/SRR059845_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059845/SRR059845_2.fastq.gz -o Fetcher/SRS017445/wget-logs/SRR059845_2.fastq.gz

