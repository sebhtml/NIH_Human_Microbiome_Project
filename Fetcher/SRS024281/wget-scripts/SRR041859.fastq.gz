if test -f Fetcher/SRS024281/contents/SRR041859.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041859/SRR041859.fastq.gz -o Fetcher/SRS024281/wget-logs/SRR041859.fastq.gz

if test $(md5sum -c Fetcher/SRS024281/checksums/SRR041859.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041859.fastq.gz Fetcher/SRS024281/contents
fi

fi
