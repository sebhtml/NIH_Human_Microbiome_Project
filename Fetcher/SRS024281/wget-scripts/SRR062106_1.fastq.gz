if test -f Fetcher/SRS024281/contents/SRR062106_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062106/SRR062106_1.fastq.gz -o Fetcher/SRS024281/wget-logs/SRR062106_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024281/checksums/SRR062106_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062106_1.fastq.gz Fetcher/SRS024281/contents
fi

fi
