if test -f Fetcher/SRS014287/contents/SRR045506.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR045/SRR045506/SRR045506.fastq.gz -o Fetcher/SRS014287/wget-logs/SRR045506.fastq.gz

if test $(md5sum -c Fetcher/SRS014287/checksums/SRR045506.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR045506.fastq.gz Fetcher/SRS014287/contents
fi

fi
