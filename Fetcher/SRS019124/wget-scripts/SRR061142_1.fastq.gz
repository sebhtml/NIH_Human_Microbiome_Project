if test -f Fetcher/SRS019124/contents/SRR061142_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061142/SRR061142_1.fastq.gz -o Fetcher/SRS019124/wget-logs/SRR061142_1.fastq.gz

