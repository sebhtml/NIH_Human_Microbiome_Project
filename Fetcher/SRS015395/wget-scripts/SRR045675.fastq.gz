if test -f Fetcher/SRS015395/contents/SRR045675.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR045/SRR045675/SRR045675.fastq.gz -o Fetcher/SRS015395/wget-logs/SRR045675.fastq.gz

if test $(md5sum -c Fetcher/SRS015395/checksums/SRR045675.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR045675.fastq.gz Fetcher/SRS015395/contents
fi

fi
