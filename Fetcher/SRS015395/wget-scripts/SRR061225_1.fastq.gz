if test -f Fetcher/SRS015395/contents/SRR061225_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061225/SRR061225_1.fastq.gz -o Fetcher/SRS015395/wget-logs/SRR061225_1.fastq.gz

if test $(md5sum -c Fetcher/SRS015395/checksums/SRR061225_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061225_1.fastq.gz Fetcher/SRS015395/contents
fi

fi
