if test -f Fetcher/SRS011126/contents/SRR062013_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062013/SRR062013_1.fastq.gz -o Fetcher/SRS011126/wget-logs/SRR062013_1.fastq.gz

if test $(md5sum -c Fetcher/SRS011126/checksums/SRR062013_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062013_1.fastq.gz Fetcher/SRS011126/contents
fi

fi
