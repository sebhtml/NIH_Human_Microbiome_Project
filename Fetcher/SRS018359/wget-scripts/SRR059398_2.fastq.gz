if test -f Fetcher/SRS018359/contents/SRR059398_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059398/SRR059398_2.fastq.gz -o Fetcher/SRS018359/wget-logs/SRR059398_2.fastq.gz

if test $(md5sum -c Fetcher/SRS018359/checksums/SRR059398_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059398_2.fastq.gz Fetcher/SRS018359/contents
fi

fi
