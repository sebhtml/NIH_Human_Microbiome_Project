if test -f Fetcher/SRS055982/contents/SRR061730.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061730/SRR061730.fastq.gz -o Fetcher/SRS055982/wget-logs/SRR061730.fastq.gz

if test $(md5sum -c Fetcher/SRS055982/checksums/SRR061730.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061730.fastq.gz Fetcher/SRS055982/contents
fi

fi
