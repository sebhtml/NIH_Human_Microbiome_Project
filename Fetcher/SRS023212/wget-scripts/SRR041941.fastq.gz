if test -f Fetcher/SRS023212/contents/SRR041941.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041941/SRR041941.fastq.gz -o Fetcher/SRS023212/wget-logs/SRR041941.fastq.gz

if test $(md5sum -c Fetcher/SRS023212/checksums/SRR041941.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041941.fastq.gz Fetcher/SRS023212/contents
fi

fi
