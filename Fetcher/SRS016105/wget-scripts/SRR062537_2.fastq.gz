if test -f Fetcher/SRS016105/contents/SRR062537_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062537/SRR062537_2.fastq.gz -o Fetcher/SRS016105/wget-logs/SRR062537_2.fastq.gz

