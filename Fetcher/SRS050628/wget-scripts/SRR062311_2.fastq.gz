if test -f Fetcher/SRS050628/contents/SRR062311_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062311/SRR062311_2.fastq.gz -o Fetcher/SRS050628/wget-logs/SRR062311_2.fastq.gz

