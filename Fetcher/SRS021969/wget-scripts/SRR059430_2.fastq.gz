if test -f Fetcher/SRS021969/contents/SRR059430_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059430/SRR059430_2.fastq.gz -o Fetcher/SRS021969/wget-logs/SRR059430_2.fastq.gz

