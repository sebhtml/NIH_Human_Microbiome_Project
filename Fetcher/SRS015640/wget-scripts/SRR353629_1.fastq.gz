if test -f Fetcher/SRS015640/contents/SRR353629_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR353/SRR353629/SRR353629_1.fastq.gz -o Fetcher/SRS015640/wget-logs/SRR353629_1.fastq.gz

