if test -f Fetcher/SRS011310/contents/SRR057301.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057301/SRR057301.fastq.gz -o Fetcher/SRS011310/wget-logs/SRR057301.fastq.gz

if test $(md5sum -c Fetcher/SRS011310/checksums/SRR057301.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057301.fastq.gz Fetcher/SRS011310/contents
fi

fi
