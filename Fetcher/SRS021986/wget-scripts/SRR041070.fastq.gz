if test -f Fetcher/SRS021986/contents/SRR041070.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041070/SRR041070.fastq.gz -o Fetcher/SRS021986/wget-logs/SRR041070.fastq.gz

if test $(md5sum -c Fetcher/SRS021986/checksums/SRR041070.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041070.fastq.gz Fetcher/SRS021986/contents
fi

fi
