	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061929/SRR061929_1.fastq.gz -o Fetcher/SRS024625/wget-logs/SRR061929_1.fastq.gz

