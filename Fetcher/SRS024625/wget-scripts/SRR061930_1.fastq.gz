if test -f Fetcher/SRS024625/contents/SRR061930_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061930/SRR061930_1.fastq.gz -o Fetcher/SRS024625/wget-logs/SRR061930_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024625/checksums/SRR061930_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061930_1.fastq.gz Fetcher/SRS024625/contents
fi

fi
