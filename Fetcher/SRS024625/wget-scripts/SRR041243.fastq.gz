if test -f Fetcher/SRS024625/contents/SRR041243.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041243/SRR041243.fastq.gz -o Fetcher/SRS024625/wget-logs/SRR041243.fastq.gz

if test $(md5sum -c Fetcher/SRS024625/checksums/SRR041243.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041243.fastq.gz Fetcher/SRS024625/contents
fi

fi
