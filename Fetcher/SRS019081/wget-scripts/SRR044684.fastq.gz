if test -f Fetcher/SRS019081/contents/SRR044684.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044684/SRR044684.fastq.gz -o Fetcher/SRS019081/wget-logs/SRR044684.fastq.gz

if test $(md5sum -c Fetcher/SRS019081/checksums/SRR044684.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044684.fastq.gz Fetcher/SRS019081/contents
fi

fi
