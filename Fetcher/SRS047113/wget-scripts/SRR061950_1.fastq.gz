if test -f Fetcher/SRS047113/contents/SRR061950_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061950/SRR061950_1.fastq.gz -o Fetcher/SRS047113/wget-logs/SRR061950_1.fastq.gz

if test $(md5sum -c Fetcher/SRS047113/checksums/SRR061950_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061950_1.fastq.gz Fetcher/SRS047113/contents
fi

fi
