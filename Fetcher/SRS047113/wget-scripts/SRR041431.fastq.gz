if test -f Fetcher/SRS047113/contents/SRR041431.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041431/SRR041431.fastq.gz -o Fetcher/SRS047113/wget-logs/SRR041431.fastq.gz

if test $(md5sum -c Fetcher/SRS047113/checksums/SRR041431.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041431.fastq.gz Fetcher/SRS047113/contents
fi

fi
