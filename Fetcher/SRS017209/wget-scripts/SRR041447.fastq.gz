if test -f Fetcher/SRS017209/contents/SRR041447.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041447/SRR041447.fastq.gz -o Fetcher/SRS017209/wget-logs/SRR041447.fastq.gz

if test $(md5sum -c Fetcher/SRS017209/checksums/SRR041447.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041447.fastq.gz Fetcher/SRS017209/contents
fi

fi
