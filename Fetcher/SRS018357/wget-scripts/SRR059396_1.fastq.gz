if test -f Fetcher/SRS018357/contents/SRR059396_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059396/SRR059396_1.fastq.gz -o Fetcher/SRS018357/wget-logs/SRR059396_1.fastq.gz

if test $(md5sum -c Fetcher/SRS018357/checksums/SRR059396_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059396_1.fastq.gz Fetcher/SRS018357/contents
fi

fi
