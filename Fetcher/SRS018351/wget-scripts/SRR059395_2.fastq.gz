if test -f Fetcher/SRS018351/contents/SRR059395_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059395/SRR059395_2.fastq.gz -o Fetcher/SRS018351/wget-logs/SRR059395_2.fastq.gz

if test $(md5sum -c Fetcher/SRS018351/checksums/SRR059395_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059395_2.fastq.gz Fetcher/SRS018351/contents
fi

fi
