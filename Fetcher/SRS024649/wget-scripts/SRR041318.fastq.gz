if test -f Fetcher/SRS024649/contents/SRR041318.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041318/SRR041318.fastq.gz -o Fetcher/SRS024649/wget-logs/SRR041318.fastq.gz

if test $(md5sum -c Fetcher/SRS024649/checksums/SRR041318.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041318.fastq.gz Fetcher/SRS024649/contents
fi

fi
