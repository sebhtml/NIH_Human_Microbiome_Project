if test -f Fetcher/SRS024649/contents/SRR061905_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061905/SRR061905_2.fastq.gz -o Fetcher/SRS024649/wget-logs/SRR061905_2.fastq.gz

if test $(md5sum -c Fetcher/SRS024649/checksums/SRR061905_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061905_2.fastq.gz Fetcher/SRS024649/contents
fi

fi
