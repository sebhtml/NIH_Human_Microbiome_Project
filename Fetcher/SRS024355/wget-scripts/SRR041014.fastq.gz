if test -f Fetcher/SRS024355/contents/SRR041014.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041014/SRR041014.fastq.gz -o Fetcher/SRS024355/wget-logs/SRR041014.fastq.gz

if test $(md5sum -c Fetcher/SRS024355/checksums/SRR041014.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041014.fastq.gz Fetcher/SRS024355/contents
fi

fi
