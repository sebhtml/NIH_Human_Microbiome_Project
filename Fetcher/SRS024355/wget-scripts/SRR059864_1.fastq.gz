if test -f Fetcher/SRS024355/contents/SRR059864_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059864/SRR059864_1.fastq.gz -o Fetcher/SRS024355/wget-logs/SRR059864_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024355/checksums/SRR059864_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059864_1.fastq.gz Fetcher/SRS024355/contents
fi

fi
