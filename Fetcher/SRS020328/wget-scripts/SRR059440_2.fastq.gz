if test -f Fetcher/SRS020328/contents/SRR059440_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059440/SRR059440_2.fastq.gz -o Fetcher/SRS020328/wget-logs/SRR059440_2.fastq.gz

