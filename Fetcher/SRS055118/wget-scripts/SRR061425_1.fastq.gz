if test -f Fetcher/SRS055118/contents/SRR061425_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061425/SRR061425_1.fastq.gz -o Fetcher/SRS055118/wget-logs/SRR061425_1.fastq.gz

