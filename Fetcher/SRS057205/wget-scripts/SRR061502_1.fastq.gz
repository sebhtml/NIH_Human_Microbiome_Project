	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061502/SRR061502_1.fastq.gz -o Fetcher/SRS057205/wget-logs/SRR061502_1.fastq.gz

