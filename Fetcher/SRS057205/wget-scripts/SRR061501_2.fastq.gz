if test -f Fetcher/SRS057205/contents/SRR061501_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061501/SRR061501_2.fastq.gz -o Fetcher/SRS057205/wget-logs/SRR061501_2.fastq.gz

if test $(md5sum -c Fetcher/SRS057205/checksums/SRR061501_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061501_2.fastq.gz Fetcher/SRS057205/contents
fi

fi
