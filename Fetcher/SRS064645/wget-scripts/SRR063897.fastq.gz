if test -f Fetcher/SRS064645/contents/SRR063897.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063897/SRR063897.fastq.gz -o Fetcher/SRS064645/wget-logs/SRR063897.fastq.gz

if test $(md5sum -c Fetcher/SRS064645/checksums/SRR063897.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR063897.fastq.gz Fetcher/SRS064645/contents
fi

fi
