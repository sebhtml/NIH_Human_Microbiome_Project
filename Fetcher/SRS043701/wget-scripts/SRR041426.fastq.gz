if test -f Fetcher/SRS043701/contents/SRR041426.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041426/SRR041426.fastq.gz -o Fetcher/SRS043701/wget-logs/SRR041426.fastq.gz

if test $(md5sum -c Fetcher/SRS043701/checksums/SRR041426.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041426.fastq.gz Fetcher/SRS043701/contents
fi

fi
