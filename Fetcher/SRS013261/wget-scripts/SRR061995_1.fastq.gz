if test -f Fetcher/SRS013261/contents/SRR061995_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061995/SRR061995_1.fastq.gz -o Fetcher/SRS013261/wget-logs/SRR061995_1.fastq.gz

