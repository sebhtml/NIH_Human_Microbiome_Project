if test -f Fetcher/SRS013261/contents/SRR040735.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040735/SRR040735.fastq.gz -o Fetcher/SRS013261/wget-logs/SRR040735.fastq.gz

if test $(md5sum -c Fetcher/SRS013261/checksums/SRR040735.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040735.fastq.gz Fetcher/SRS013261/contents
fi

fi
