if test -f Fetcher/SRS062427/contents/SRR046888.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046888/SRR046888.fastq.gz -o Fetcher/SRS062427/wget-logs/SRR046888.fastq.gz

if test $(md5sum -c Fetcher/SRS062427/checksums/SRR046888.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046888.fastq.gz Fetcher/SRS062427/contents
fi

fi
