if test -f Fetcher/SRS045004/contents/SRR060152_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060152/SRR060152_1.fastq.gz -o Fetcher/SRS045004/wget-logs/SRR060152_1.fastq.gz

if test $(md5sum -c Fetcher/SRS045004/checksums/SRR060152_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060152_1.fastq.gz Fetcher/SRS045004/contents
