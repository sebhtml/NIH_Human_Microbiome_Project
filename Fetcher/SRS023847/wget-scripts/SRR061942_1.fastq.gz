if test -f Fetcher/SRS023847/contents/SRR061942_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061942/SRR061942_1.fastq.gz -o Fetcher/SRS023847/wget-logs/SRR061942_1.fastq.gz

if test $(md5sum -c Fetcher/SRS023847/checksums/SRR061942_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061942_1.fastq.gz Fetcher/SRS023847/contents
fi

fi
