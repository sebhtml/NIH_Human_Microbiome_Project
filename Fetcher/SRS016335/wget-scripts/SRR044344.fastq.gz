if test -f Fetcher/SRS016335/contents/SRR044344.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044344/SRR044344.fastq.gz -o Fetcher/SRS016335/wget-logs/SRR044344.fastq.gz

if test $(md5sum -c Fetcher/SRS016335/checksums/SRR044344.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044344.fastq.gz Fetcher/SRS016335/contents
fi

fi
