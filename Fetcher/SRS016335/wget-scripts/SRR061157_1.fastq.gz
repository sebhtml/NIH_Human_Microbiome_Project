if test -f Fetcher/SRS016335/contents/SRR061157_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061157/SRR061157_1.fastq.gz -o Fetcher/SRS016335/wget-logs/SRR061157_1.fastq.gz

