if test -f Fetcher/SRS019787/contents/SRR043980.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR043/SRR043980/SRR043980.fastq.gz -o Fetcher/SRS019787/wget-logs/SRR043980.fastq.gz

if test $(md5sum -c Fetcher/SRS019787/checksums/SRR043980.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR043980.fastq.gz Fetcher/SRS019787/contents
