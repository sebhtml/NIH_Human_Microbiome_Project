if test -f Fetcher/SRS042643/contents/SRR046038.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046038/SRR046038.fastq.gz -o Fetcher/SRS042643/wget-logs/SRR046038.fastq.gz

if test $(md5sum -c Fetcher/SRS042643/checksums/SRR046038.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046038.fastq.gz Fetcher/SRS042643/contents
fi

fi
