if test -f Fetcher/SRS024424/contents/SRR040671.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040671/SRR040671.fastq.gz -o Fetcher/SRS024424/wget-logs/SRR040671.fastq.gz

if test $(md5sum -c Fetcher/SRS024424/checksums/SRR040671.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040671.fastq.gz Fetcher/SRS024424/contents
fi

fi
