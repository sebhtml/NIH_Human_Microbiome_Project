if test -f Fetcher/SRS024424/contents/SRR063925_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063925/SRR063925_1.fastq.gz -o Fetcher/SRS024424/wget-logs/SRR063925_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024424/checksums/SRR063925_1.fastq.gz|grep OK$|wc -l) = 1
