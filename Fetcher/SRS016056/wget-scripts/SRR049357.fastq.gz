if test -f Fetcher/SRS016056/contents/SRR049357.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049357/SRR049357.fastq.gz -o Fetcher/SRS016056/wget-logs/SRR049357.fastq.gz

if test $(md5sum -c Fetcher/SRS016056/checksums/SRR049357.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049357.fastq.gz Fetcher/SRS016056/contents
fi

fi
