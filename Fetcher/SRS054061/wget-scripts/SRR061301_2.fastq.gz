if test -f Fetcher/SRS054061/contents/SRR061301_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061301/SRR061301_2.fastq.gz -o Fetcher/SRS054061/wget-logs/SRR061301_2.fastq.gz

if test $(md5sum -c Fetcher/SRS054061/checksums/SRR061301_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061301_2.fastq.gz Fetcher/SRS054061/contents
fi

fi
