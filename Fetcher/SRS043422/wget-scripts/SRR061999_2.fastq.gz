if test -f Fetcher/SRS043422/contents/SRR061999_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061999/SRR061999_2.fastq.gz -o Fetcher/SRS043422/wget-logs/SRR061999_2.fastq.gz

if test $(md5sum -c Fetcher/SRS043422/checksums/SRR061999_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061999_2.fastq.gz Fetcher/SRS043422/contents
fi

fi
