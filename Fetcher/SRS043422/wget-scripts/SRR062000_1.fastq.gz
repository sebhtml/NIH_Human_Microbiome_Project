if test -f Fetcher/SRS043422/contents/SRR062000_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062000/SRR062000_1.fastq.gz -o Fetcher/SRS043422/wget-logs/SRR062000_1.fastq.gz

if test $(md5sum -c Fetcher/SRS043422/checksums/SRR062000_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062000_1.fastq.gz Fetcher/SRS043422/contents
fi

fi
