if test -f Fetcher/SRS078197/contents/SRR061736.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061736/SRR061736.fastq.gz -o Fetcher/SRS078197/wget-logs/SRR061736.fastq.gz

if test $(md5sum -c Fetcher/SRS078197/checksums/SRR061736.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061736.fastq.gz Fetcher/SRS078197/contents
fi

fi
