if test -f Fetcher/SRS013502/contents/SRR040670.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040670/SRR040670.fastq.gz -o Fetcher/SRS013502/wget-logs/SRR040670.fastq.gz

if test $(md5sum -c Fetcher/SRS013502/checksums/SRR040670.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040670.fastq.gz Fetcher/SRS013502/contents
fi

fi
