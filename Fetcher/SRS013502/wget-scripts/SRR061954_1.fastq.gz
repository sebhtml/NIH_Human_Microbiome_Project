if test -f Fetcher/SRS013502/contents/SRR061954_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061954/SRR061954_1.fastq.gz -o Fetcher/SRS013502/wget-logs/SRR061954_1.fastq.gz

if test $(md5sum -c Fetcher/SRS013502/checksums/SRR061954_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061954_1.fastq.gz Fetcher/SRS013502/contents
fi

fi
