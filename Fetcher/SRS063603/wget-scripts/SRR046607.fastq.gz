if test -f Fetcher/SRS063603/contents/SRR046607.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046607/SRR046607.fastq.gz -o Fetcher/SRS063603/wget-logs/SRR046607.fastq.gz

if test $(md5sum -c Fetcher/SRS063603/checksums/SRR046607.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046607.fastq.gz Fetcher/SRS063603/contents
fi

fi
