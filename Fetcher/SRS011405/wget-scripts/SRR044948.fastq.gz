if test -f Fetcher/SRS011405/contents/SRR044948.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044948/SRR044948.fastq.gz -o Fetcher/SRS011405/wget-logs/SRR044948.fastq.gz

if test $(md5sum -c Fetcher/SRS011405/checksums/SRR044948.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044948.fastq.gz Fetcher/SRS011405/contents
fi

fi
