if test -f Fetcher/SRS013164/contents/SRR059848_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059848/SRR059848_1.fastq.gz -o Fetcher/SRS013164/wget-logs/SRR059848_1.fastq.gz

