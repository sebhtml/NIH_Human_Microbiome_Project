if test -f Fetcher/SRS022524/contents/SRR063903.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063903/SRR063903.fastq.gz -o Fetcher/SRS022524/wget-logs/SRR063903.fastq.gz

if test $(md5sum -c Fetcher/SRS022524/checksums/SRR063903.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR063903.fastq.gz Fetcher/SRS022524/contents
fi

fi
