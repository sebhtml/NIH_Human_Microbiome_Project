if test -f Fetcher/SRS017497/contents/SRR041213.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041213/SRR041213.fastq.gz -o Fetcher/SRS017497/wget-logs/SRR041213.fastq.gz

if test $(md5sum -c Fetcher/SRS017497/checksums/SRR041213.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041213.fastq.gz Fetcher/SRS017497/contents
fi

fi
