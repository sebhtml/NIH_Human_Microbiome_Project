if test -f Fetcher/SRS062540/contents/SRR061485_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061485/SRR061485_2.fastq.gz -o Fetcher/SRS062540/wget-logs/SRR061485_2.fastq.gz

