if test -f Fetcher/SRS062540/contents/SRR047347.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047347/SRR047347.fastq.gz -o Fetcher/SRS062540/wget-logs/SRR047347.fastq.gz

if test $(md5sum -c Fetcher/SRS062540/checksums/SRR047347.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047347.fastq.gz Fetcher/SRS062540/contents
fi

fi
