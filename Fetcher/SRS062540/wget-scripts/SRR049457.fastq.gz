if test -f Fetcher/SRS062540/contents/SRR049457.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049457/SRR049457.fastq.gz -o Fetcher/SRS062540/wget-logs/SRR049457.fastq.gz

if test $(md5sum -c Fetcher/SRS062540/checksums/SRR049457.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049457.fastq.gz Fetcher/SRS062540/contents
fi

fi
