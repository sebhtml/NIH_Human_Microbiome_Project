if test -f Fetcher/SRS020869/contents/SRR059357_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059357/SRR059357_2.fastq.gz -o Fetcher/SRS020869/wget-logs/SRR059357_2.fastq.gz

if test $(md5sum -c Fetcher/SRS020869/checksums/SRR059357_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059357_2.fastq.gz Fetcher/SRS020869/contents
fi

fi
