if test -f Fetcher/SRS017849/contents/SRR041626.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041626/SRR041626.fastq.gz -o Fetcher/SRS017849/wget-logs/SRR041626.fastq.gz

if test $(md5sum -c Fetcher/SRS017849/checksums/SRR041626.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041626.fastq.gz Fetcher/SRS017849/contents
fi

fi
