if test -f Fetcher/SRS017013/contents/SRR059895_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059895/SRR059895_1.fastq.gz -o Fetcher/SRS017013/wget-logs/SRR059895_1.fastq.gz

