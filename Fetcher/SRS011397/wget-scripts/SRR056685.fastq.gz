if test -f Fetcher/SRS011397/contents/SRR056685.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR056/SRR056685/SRR056685.fastq.gz -o Fetcher/SRS011397/wget-logs/SRR056685.fastq.gz

if test $(md5sum -c Fetcher/SRS011397/checksums/SRR056685.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR056685.fastq.gz Fetcher/SRS011397/contents
