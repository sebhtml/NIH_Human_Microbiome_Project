if test -f Fetcher/SRS011144/contents/SRR062032_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062032/SRR062032_2.fastq.gz -o Fetcher/SRS011144/wget-logs/SRR062032_2.fastq.gz

