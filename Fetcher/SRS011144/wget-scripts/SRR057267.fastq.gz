if test -f Fetcher/SRS011144/contents/SRR057267.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057267/SRR057267.fastq.gz -o Fetcher/SRS011144/wget-logs/SRR057267.fastq.gz

if test $(md5sum -c Fetcher/SRS011144/checksums/SRR057267.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057267.fastq.gz Fetcher/SRS011144/contents
fi

fi
