if test -f Fetcher/SRS011144/contents/SRR056562.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR056/SRR056562/SRR056562.fastq.gz -o Fetcher/SRS011144/wget-logs/SRR056562.fastq.gz

if test $(md5sum -c Fetcher/SRS011144/checksums/SRR056562.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR056562.fastq.gz Fetcher/SRS011144/contents
fi

fi
