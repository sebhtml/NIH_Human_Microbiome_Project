if test -f Fetcher/SRS011144/contents/SRR057193.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057193/SRR057193.fastq.gz -o Fetcher/SRS011144/wget-logs/SRR057193.fastq.gz

if test $(md5sum -c Fetcher/SRS011144/checksums/SRR057193.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057193.fastq.gz Fetcher/SRS011144/contents
fi

fi
