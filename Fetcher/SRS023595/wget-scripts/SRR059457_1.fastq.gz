if test -f Fetcher/SRS023595/contents/SRR059457_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059457/SRR059457_1.fastq.gz -o Fetcher/SRS023595/wget-logs/SRR059457_1.fastq.gz

if test $(md5sum -c Fetcher/SRS023595/checksums/SRR059457_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059457_1.fastq.gz Fetcher/SRS023595/contents
fi

fi
