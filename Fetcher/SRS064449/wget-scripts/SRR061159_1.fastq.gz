if test -f Fetcher/SRS064449/contents/SRR061159_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061159/SRR061159_1.fastq.gz -o Fetcher/SRS064449/wget-logs/SRR061159_1.fastq.gz

