if test -f Fetcher/SRS017076/contents/SRR060431_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060431/SRR060431_1.fastq.gz -o Fetcher/SRS017076/wget-logs/SRR060431_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017076/checksums/SRR060431_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060431_1.fastq.gz Fetcher/SRS017076/contents
fi

fi
