if test -f Fetcher/SRS013158/contents/SRR059896_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059896/SRR059896_2.fastq.gz -o Fetcher/SRS013158/wget-logs/SRR059896_2.fastq.gz

if test $(md5sum -c Fetcher/SRS013158/checksums/SRR059896_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059896_2.fastq.gz Fetcher/SRS013158/contents
fi

fi
