if test -f Fetcher/SRS019019/contents/SRR061471_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061471/SRR061471_2.fastq.gz -o Fetcher/SRS019019/wget-logs/SRR061471_2.fastq.gz

