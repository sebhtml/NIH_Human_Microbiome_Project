if test -f Fetcher/SRS011152/contents/SRR057197.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057197/SRR057197.fastq.gz -o Fetcher/SRS011152/wget-logs/SRR057197.fastq.gz

if test $(md5sum -c Fetcher/SRS011152/checksums/SRR057197.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057197.fastq.gz Fetcher/SRS011152/contents
fi

fi
