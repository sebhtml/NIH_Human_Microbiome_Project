if test -f Fetcher/SRS052697/contents/SRR049639.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049639/SRR049639.fastq.gz -o Fetcher/SRS052697/wget-logs/SRR049639.fastq.gz

if test $(md5sum -c Fetcher/SRS052697/checksums/SRR049639.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049639.fastq.gz Fetcher/SRS052697/contents
fi

fi
