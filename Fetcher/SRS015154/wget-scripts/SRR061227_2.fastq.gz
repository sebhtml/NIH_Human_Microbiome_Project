if test -f Fetcher/SRS015154/contents/SRR061227_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061227/SRR061227_2.fastq.gz -o Fetcher/SRS015154/wget-logs/SRR061227_2.fastq.gz

if test $(md5sum -c Fetcher/SRS015154/checksums/SRR061227_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061227_2.fastq.gz Fetcher/SRS015154/contents
