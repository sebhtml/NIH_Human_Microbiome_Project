if test -f Fetcher/SRS016585/contents/SRR042899.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042899/SRR042899.fastq.gz -o Fetcher/SRS016585/wget-logs/SRR042899.fastq.gz

if test $(md5sum -c Fetcher/SRS016585/checksums/SRR042899.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042899.fastq.gz Fetcher/SRS016585/contents
fi

fi
