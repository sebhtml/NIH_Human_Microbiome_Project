if test -f Fetcher/SRS016585/contents/SRR043223.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR043/SRR043223/SRR043223.fastq.gz -o Fetcher/SRS016585/wget-logs/SRR043223.fastq.gz

if test $(md5sum -c Fetcher/SRS016585/checksums/SRR043223.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR043223.fastq.gz Fetcher/SRS016585/contents
fi

fi
