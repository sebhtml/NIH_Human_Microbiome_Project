if test -f Fetcher/SRS016434/contents/SRR042800.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042800/SRR042800.fastq.gz -o Fetcher/SRS016434/wget-logs/SRR042800.fastq.gz

if test $(md5sum -c Fetcher/SRS016434/checksums/SRR042800.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042800.fastq.gz Fetcher/SRS016434/contents
fi

fi
