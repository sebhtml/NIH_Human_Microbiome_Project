if test -f Fetcher/SRS015174/contents/SRR061473_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061473/SRR061473_1.fastq.gz -o Fetcher/SRS015174/wget-logs/SRR061473_1.fastq.gz

