if test -f Fetcher/SRS015174/contents/SRR061245_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061245/SRR061245_1.fastq.gz -o Fetcher/SRS015174/wget-logs/SRR061245_1.fastq.gz

if test $(md5sum -c Fetcher/SRS015174/checksums/SRR061245_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061245_1.fastq.gz Fetcher/SRS015174/contents
fi

fi
