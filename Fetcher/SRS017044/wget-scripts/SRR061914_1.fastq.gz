if test -f Fetcher/SRS017044/contents/SRR061914_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061914/SRR061914_1.fastq.gz -o Fetcher/SRS017044/wget-logs/SRR061914_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017044/checksums/SRR061914_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061914_1.fastq.gz Fetcher/SRS017044/contents
fi

fi
