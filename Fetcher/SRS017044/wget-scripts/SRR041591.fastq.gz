if test -f Fetcher/SRS017044/contents/SRR041591.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041591/SRR041591.fastq.gz -o Fetcher/SRS017044/wget-logs/SRR041591.fastq.gz

if test $(md5sum -c Fetcher/SRS017044/checksums/SRR041591.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041591.fastq.gz Fetcher/SRS017044/contents
fi

fi
