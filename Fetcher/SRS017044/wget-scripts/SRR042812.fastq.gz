if test -f Fetcher/SRS017044/contents/SRR042812.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042812/SRR042812.fastq.gz -o Fetcher/SRS017044/wget-logs/SRR042812.fastq.gz

if test $(md5sum -c Fetcher/SRS017044/checksums/SRR042812.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042812.fastq.gz Fetcher/SRS017044/contents
fi

fi
