if test -f Fetcher/SRS015921/contents/SRR044611.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044611/SRR044611.fastq.gz -o Fetcher/SRS015921/wget-logs/SRR044611.fastq.gz

if test $(md5sum -c Fetcher/SRS015921/checksums/SRR044611.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044611.fastq.gz Fetcher/SRS015921/contents
fi

fi
