if test -f Fetcher/SRS024482/contents/SRR061947_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061947/SRR061947_1.fastq.gz -o Fetcher/SRS024482/wget-logs/SRR061947_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024482/checksums/SRR061947_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061947_1.fastq.gz Fetcher/SRS024482/contents
fi

fi
