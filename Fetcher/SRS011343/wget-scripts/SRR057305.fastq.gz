if test -f Fetcher/SRS011343/contents/SRR057305.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR057/SRR057305/SRR057305.fastq.gz -o Fetcher/SRS011343/wget-logs/SRR057305.fastq.gz

if test $(md5sum -c Fetcher/SRS011343/checksums/SRR057305.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR057305.fastq.gz Fetcher/SRS011343/contents
fi

fi
