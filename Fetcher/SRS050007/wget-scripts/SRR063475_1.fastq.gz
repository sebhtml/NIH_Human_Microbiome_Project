if test -f Fetcher/SRS050007/contents/SRR063475_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR063/SRR063475/SRR063475_1.fastq.gz -o Fetcher/SRS050007/wget-logs/SRR063475_1.fastq.gz

if test $(md5sum -c Fetcher/SRS050007/checksums/SRR063475_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR063475_1.fastq.gz Fetcher/SRS050007/contents
