if test -f Fetcher/SRS012281/contents/SRR059449_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059449/SRR059449_1.fastq.gz -o Fetcher/SRS012281/wget-logs/SRR059449_1.fastq.gz

if test $(md5sum -c Fetcher/SRS012281/checksums/SRR059449_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059449_1.fastq.gz Fetcher/SRS012281/contents
fi

fi
