if test -f Fetcher/SRS024388/contents/SRR041171.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041171/SRR041171.fastq.gz -o Fetcher/SRS024388/wget-logs/SRR041171.fastq.gz

if test $(md5sum -c Fetcher/SRS024388/checksums/SRR041171.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041171.fastq.gz Fetcher/SRS024388/contents
fi

fi
