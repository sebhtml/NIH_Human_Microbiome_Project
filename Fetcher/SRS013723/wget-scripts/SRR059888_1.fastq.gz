if test -f Fetcher/SRS013723/contents/SRR059888_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059888/SRR059888_1.fastq.gz -o Fetcher/SRS013723/wget-logs/SRR059888_1.fastq.gz

