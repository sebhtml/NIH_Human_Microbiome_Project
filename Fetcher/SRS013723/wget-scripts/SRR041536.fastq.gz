if test -f Fetcher/SRS013723/contents/SRR041536.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041536/SRR041536.fastq.gz -o Fetcher/SRS013723/wget-logs/SRR041536.fastq.gz

if test $(md5sum -c Fetcher/SRS013723/checksums/SRR041536.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041536.fastq.gz Fetcher/SRS013723/contents
fi

fi
