if test -f Fetcher/SRS013800/contents/SRR059338_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059338/SRR059338_1.fastq.gz -o Fetcher/SRS013800/wget-logs/SRR059338_1.fastq.gz

if test $(md5sum -c Fetcher/SRS013800/checksums/SRR059338_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059338_1.fastq.gz Fetcher/SRS013800/contents
fi

fi
