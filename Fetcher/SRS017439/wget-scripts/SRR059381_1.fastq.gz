if test -f Fetcher/SRS017439/contents/SRR059381_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059381/SRR059381_1.fastq.gz -o Fetcher/SRS017439/wget-logs/SRR059381_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017439/checksums/SRR059381_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059381_1.fastq.gz Fetcher/SRS017439/contents
fi

fi
