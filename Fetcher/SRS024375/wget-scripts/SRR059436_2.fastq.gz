if test -f Fetcher/SRS024375/contents/SRR059436_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059436/SRR059436_2.fastq.gz -o Fetcher/SRS024375/wget-logs/SRR059436_2.fastq.gz

if test $(md5sum -c Fetcher/SRS024375/checksums/SRR059436_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059436_2.fastq.gz Fetcher/SRS024375/contents
fi

fi
