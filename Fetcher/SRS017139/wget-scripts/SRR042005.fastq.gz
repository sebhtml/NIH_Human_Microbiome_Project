if test -f Fetcher/SRS017139/contents/SRR042005.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR042/SRR042005/SRR042005.fastq.gz -o Fetcher/SRS017139/wget-logs/SRR042005.fastq.gz

if test $(md5sum -c Fetcher/SRS017139/checksums/SRR042005.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR042005.fastq.gz Fetcher/SRS017139/contents
fi

fi
