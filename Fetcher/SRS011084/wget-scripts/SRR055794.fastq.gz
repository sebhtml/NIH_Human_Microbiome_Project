if test -f Fetcher/SRS011084/contents/SRR055794.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR055/SRR055794/SRR055794.fastq.gz -o Fetcher/SRS011084/wget-logs/SRR055794.fastq.gz

if test $(md5sum -c Fetcher/SRS011084/checksums/SRR055794.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR055794.fastq.gz Fetcher/SRS011084/contents
fi

fi
