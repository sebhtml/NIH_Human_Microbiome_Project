if test -f Fetcher/SRS015755/contents/SRR061521_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061521/SRR061521_1.fastq.gz -o Fetcher/SRS015755/wget-logs/SRR061521_1.fastq.gz

if test $(md5sum -c Fetcher/SRS015755/checksums/SRR061521_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061521_1.fastq.gz Fetcher/SRS015755/contents
fi

fi
