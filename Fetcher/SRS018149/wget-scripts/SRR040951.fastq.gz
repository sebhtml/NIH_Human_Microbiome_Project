if test -f Fetcher/SRS018149/contents/SRR040951.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040951/SRR040951.fastq.gz -o Fetcher/SRS018149/wget-logs/SRR040951.fastq.gz

if test $(md5sum -c Fetcher/SRS018149/checksums/SRR040951.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040951.fastq.gz Fetcher/SRS018149/contents
fi

fi
