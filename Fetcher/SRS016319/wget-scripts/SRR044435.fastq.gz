if test -f Fetcher/SRS016319/contents/SRR044435.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044435/SRR044435.fastq.gz -o Fetcher/SRS016319/wget-logs/SRR044435.fastq.gz

if test $(md5sum -c Fetcher/SRS016319/checksums/SRR044435.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044435.fastq.gz Fetcher/SRS016319/contents
fi

fi
