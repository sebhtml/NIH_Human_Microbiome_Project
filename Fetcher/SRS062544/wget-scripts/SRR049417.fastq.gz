if test -f Fetcher/SRS062544/contents/SRR049417.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049417/SRR049417.fastq.gz -o Fetcher/SRS062544/wget-logs/SRR049417.fastq.gz

if test $(md5sum -c Fetcher/SRS062544/checksums/SRR049417.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049417.fastq.gz Fetcher/SRS062544/contents
fi

fi
