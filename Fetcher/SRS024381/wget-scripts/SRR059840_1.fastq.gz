if test -f Fetcher/SRS024381/contents/SRR059840_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059840/SRR059840_1.fastq.gz -o Fetcher/SRS024381/wget-logs/SRR059840_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024381/checksums/SRR059840_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059840_1.fastq.gz Fetcher/SRS024381/contents
fi

fi
