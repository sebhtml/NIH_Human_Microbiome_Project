if test -f Fetcher/SRS024347/contents/SRR061970_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061970/SRR061970_2.fastq.gz -o Fetcher/SRS024347/wget-logs/SRR061970_2.fastq.gz

if test $(md5sum -c Fetcher/SRS024347/checksums/SRR061970_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061970_2.fastq.gz Fetcher/SRS024347/contents
fi

fi
