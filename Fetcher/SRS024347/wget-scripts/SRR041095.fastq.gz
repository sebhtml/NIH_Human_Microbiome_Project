if test -f Fetcher/SRS024347/contents/SRR041095.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041095/SRR041095.fastq.gz -o Fetcher/SRS024347/wget-logs/SRR041095.fastq.gz

if test $(md5sum -c Fetcher/SRS024347/checksums/SRR041095.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041095.fastq.gz Fetcher/SRS024347/contents
fi

fi
