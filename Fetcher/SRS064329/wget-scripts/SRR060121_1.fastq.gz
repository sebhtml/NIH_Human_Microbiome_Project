if test -f Fetcher/SRS064329/contents/SRR060121_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060121/SRR060121_1.fastq.gz -o Fetcher/SRS064329/wget-logs/SRR060121_1.fastq.gz

