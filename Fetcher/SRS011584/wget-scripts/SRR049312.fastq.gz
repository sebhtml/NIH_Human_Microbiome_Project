if test -f Fetcher/SRS011584/contents/SRR049312.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR049/SRR049312/SRR049312.fastq.gz -o Fetcher/SRS011584/wget-logs/SRR049312.fastq.gz

if test $(md5sum -c Fetcher/SRS011584/checksums/SRR049312.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR049312.fastq.gz Fetcher/SRS011584/contents
fi

fi
