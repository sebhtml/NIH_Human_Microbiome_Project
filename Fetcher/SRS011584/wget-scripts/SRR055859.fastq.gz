if test -f Fetcher/SRS011584/contents/SRR055859.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR055/SRR055859/SRR055859.fastq.gz -o Fetcher/SRS011584/wget-logs/SRR055859.fastq.gz

if test $(md5sum -c Fetcher/SRS011584/checksums/SRR055859.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR055859.fastq.gz Fetcher/SRS011584/contents
fi

fi
