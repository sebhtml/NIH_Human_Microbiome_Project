if test -f Fetcher/SRS020856/contents/SRR059500_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059500/SRR059500_1.fastq.gz -o Fetcher/SRS020856/wget-logs/SRR059500_1.fastq.gz

if test $(md5sum -c Fetcher/SRS020856/checksums/SRR059500_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059500_1.fastq.gz Fetcher/SRS020856/contents
fi

fi
