if test -f Fetcher/SRS023176/contents/SRR041953.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041953/SRR041953.fastq.gz -o Fetcher/SRS023176/wget-logs/SRR041953.fastq.gz

if test $(md5sum -c Fetcher/SRS023176/checksums/SRR041953.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041953.fastq.gz Fetcher/SRS023176/contents
fi

fi
