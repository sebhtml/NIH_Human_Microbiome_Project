if test -f Fetcher/SRS023176/contents/SRR059900_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059900/SRR059900_1.fastq.gz -o Fetcher/SRS023176/wget-logs/SRR059900_1.fastq.gz

