if test -f Fetcher/SRS024567/contents/SRR059387_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059387/SRR059387_2.fastq.gz -o Fetcher/SRS024567/wget-logs/SRR059387_2.fastq.gz

if test $(md5sum -c Fetcher/SRS024567/checksums/SRR059387_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059387_2.fastq.gz Fetcher/SRS024567/contents
fi

fi
