if test -f Fetcher/SRS019087/contents/SRR044642.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044642/SRR044642.fastq.gz -o Fetcher/SRS019087/wget-logs/SRR044642.fastq.gz

if test $(md5sum -c Fetcher/SRS019087/checksums/SRR044642.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044642.fastq.gz Fetcher/SRS019087/contents
fi

fi
