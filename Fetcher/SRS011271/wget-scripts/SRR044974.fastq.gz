if test -f Fetcher/SRS011271/contents/SRR044974.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044974/SRR044974.fastq.gz -o Fetcher/SRS011271/wget-logs/SRR044974.fastq.gz

if test $(md5sum -c Fetcher/SRS011271/checksums/SRR044974.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044974.fastq.gz Fetcher/SRS011271/contents
fi

fi
