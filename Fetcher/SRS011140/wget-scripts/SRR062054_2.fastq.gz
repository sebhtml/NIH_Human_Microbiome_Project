if test -f Fetcher/SRS011140/contents/SRR062054_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062054/SRR062054_2.fastq.gz -o Fetcher/SRS011140/wget-logs/SRR062054_2.fastq.gz

if test $(md5sum -c Fetcher/SRS011140/checksums/SRR062054_2.fastq.gz|grep OK$|wc -l) = 1
