if test -f Fetcher/SRS024138/contents/SRR059509_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059509/SRR059509_1.fastq.gz -o Fetcher/SRS024138/wget-logs/SRR059509_1.fastq.gz

if test $(md5sum -c Fetcher/SRS024138/checksums/SRR059509_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059509_1.fastq.gz Fetcher/SRS024138/contents
fi

fi
