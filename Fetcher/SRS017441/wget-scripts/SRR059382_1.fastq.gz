if test -f Fetcher/SRS017441/contents/SRR059382_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059382/SRR059382_1.fastq.gz -o Fetcher/SRS017441/wget-logs/SRR059382_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017441/checksums/SRR059382_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059382_1.fastq.gz Fetcher/SRS017441/contents
fi

fi
