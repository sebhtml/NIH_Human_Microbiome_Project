if test -f Fetcher/SRS051031/contents/SRR061234_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061234/SRR061234_1.fastq.gz -o Fetcher/SRS051031/wget-logs/SRR061234_1.fastq.gz

if test $(md5sum -c Fetcher/SRS051031/checksums/SRR061234_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061234_1.fastq.gz Fetcher/SRS051031/contents
fi

fi
