if test -f Fetcher/SRS051031/contents/SRR047244.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR047/SRR047244/SRR047244.fastq.gz -o Fetcher/SRS051031/wget-logs/SRR047244.fastq.gz

if test $(md5sum -c Fetcher/SRS051031/checksums/SRR047244.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR047244.fastq.gz Fetcher/SRS051031/contents
fi

fi
