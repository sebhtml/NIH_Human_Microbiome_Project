if test -f Fetcher/SRS017701/contents/SRR059353_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059353/SRR059353_1.fastq.gz -o Fetcher/SRS017701/wget-logs/SRR059353_1.fastq.gz

if test $(md5sum -c Fetcher/SRS017701/checksums/SRR059353_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059353_1.fastq.gz Fetcher/SRS017701/contents
fi

fi
