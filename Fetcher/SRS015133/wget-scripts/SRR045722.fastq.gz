if test -f Fetcher/SRS015133/contents/SRR045722.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR045/SRR045722/SRR045722.fastq.gz -o Fetcher/SRS015133/wget-logs/SRR045722.fastq.gz

if test $(md5sum -c Fetcher/SRS015133/checksums/SRR045722.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR045722.fastq.gz Fetcher/SRS015133/contents
fi

fi
