if test -f Fetcher/SRS015133/contents/SRR061182_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061182/SRR061182_2.fastq.gz -o Fetcher/SRS015133/wget-logs/SRR061182_2.fastq.gz

if test $(md5sum -c Fetcher/SRS015133/checksums/SRR061182_2.fastq.gz|grep OK$|wc -l) = 1
