if test -f Fetcher/SRS022625/contents/SRR061965_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061965/SRR061965_1.fastq.gz -o Fetcher/SRS022625/wget-logs/SRR061965_1.fastq.gz

if test $(md5sum -c Fetcher/SRS022625/checksums/SRR061965_1.fastq.gz|grep OK$|wc -l) = 1
