if test -f Fetcher/SRS045197/contents/SRR061240_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061240/SRR061240_2.fastq.gz -o Fetcher/SRS045197/wget-logs/SRR061240_2.fastq.gz

if test $(md5sum -c Fetcher/SRS045197/checksums/SRR061240_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061240_2.fastq.gz Fetcher/SRS045197/contents
fi

fi
