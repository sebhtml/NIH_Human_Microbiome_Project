if test -f Fetcher/SRS018133/contents/SRR061973_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061973/SRR061973_1.fastq.gz -o Fetcher/SRS018133/wget-logs/SRR061973_1.fastq.gz

