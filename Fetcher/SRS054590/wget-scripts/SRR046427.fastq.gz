if test -f Fetcher/SRS054590/contents/SRR046427.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046427/SRR046427.fastq.gz -o Fetcher/SRS054590/wget-logs/SRR046427.fastq.gz

if test $(md5sum -c Fetcher/SRS054590/checksums/SRR046427.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046427.fastq.gz Fetcher/SRS054590/contents
fi

fi
