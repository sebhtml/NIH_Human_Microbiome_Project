if test -f Fetcher/SRS020862/contents/SRR059842_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059842/SRR059842_1.fastq.gz -o Fetcher/SRS020862/wget-logs/SRR059842_1.fastq.gz

if test $(md5sum -c Fetcher/SRS020862/checksums/SRR059842_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059842_1.fastq.gz Fetcher/SRS020862/contents
fi

fi
