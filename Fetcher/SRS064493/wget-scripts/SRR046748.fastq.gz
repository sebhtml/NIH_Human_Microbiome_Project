if test -f Fetcher/SRS064493/contents/SRR046748.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046748/SRR046748.fastq.gz -o Fetcher/SRS064493/wget-logs/SRR046748.fastq.gz

if test $(md5sum -c Fetcher/SRS064493/checksums/SRR046748.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046748.fastq.gz Fetcher/SRS064493/contents
fi

fi
