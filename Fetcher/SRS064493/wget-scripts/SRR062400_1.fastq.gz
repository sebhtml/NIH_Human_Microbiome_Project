if test -f Fetcher/SRS064493/contents/SRR062400_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062400/SRR062400_1.fastq.gz -o Fetcher/SRS064493/wget-logs/SRR062400_1.fastq.gz

if test $(md5sum -c Fetcher/SRS064493/checksums/SRR062400_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062400_1.fastq.gz Fetcher/SRS064493/contents
fi

fi
