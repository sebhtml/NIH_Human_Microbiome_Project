if test -f Fetcher/SRS064493/contents/SRR046544.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046544/SRR046544.fastq.gz -o Fetcher/SRS064493/wget-logs/SRR046544.fastq.gz

if test $(md5sum -c Fetcher/SRS064493/checksums/SRR046544.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046544.fastq.gz Fetcher/SRS064493/contents
fi

fi
