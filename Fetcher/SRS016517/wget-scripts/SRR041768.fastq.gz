if test -f Fetcher/SRS016517/contents/SRR041768.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041768/SRR041768.fastq.gz -o Fetcher/SRS016517/wget-logs/SRR041768.fastq.gz

if test $(md5sum -c Fetcher/SRS016517/checksums/SRR041768.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041768.fastq.gz Fetcher/SRS016517/contents
fi

fi
