if test -f Fetcher/SRS024470/contents/SRR041082.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041082/SRR041082.fastq.gz -o Fetcher/SRS024470/wget-logs/SRR041082.fastq.gz

if test $(md5sum -c Fetcher/SRS024470/checksums/SRR041082.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041082.fastq.gz Fetcher/SRS024470/contents
fi

fi
