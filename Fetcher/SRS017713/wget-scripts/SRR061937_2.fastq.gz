if test -f Fetcher/SRS017713/contents/SRR061937_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061937/SRR061937_2.fastq.gz -o Fetcher/SRS017713/wget-logs/SRR061937_2.fastq.gz

