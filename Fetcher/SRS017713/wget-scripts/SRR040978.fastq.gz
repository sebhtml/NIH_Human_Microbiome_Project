if test -f Fetcher/SRS017713/contents/SRR040978.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040978/SRR040978.fastq.gz -o Fetcher/SRS017713/wget-logs/SRR040978.fastq.gz

if test $(md5sum -c Fetcher/SRS017713/checksums/SRR040978.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040978.fastq.gz Fetcher/SRS017713/contents
fi

fi
