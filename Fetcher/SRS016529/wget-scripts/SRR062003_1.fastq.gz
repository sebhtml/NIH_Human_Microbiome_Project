if test -f Fetcher/SRS016529/contents/SRR062003_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062003/SRR062003_1.fastq.gz -o Fetcher/SRS016529/wget-logs/SRR062003_1.fastq.gz

if test $(md5sum -c Fetcher/SRS016529/checksums/SRR062003_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062003_1.fastq.gz Fetcher/SRS016529/contents
fi

fi
