if test -f Fetcher/SRS013533/contents/SRR059815_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059815/SRR059815_1.fastq.gz -o Fetcher/SRS013533/wget-logs/SRR059815_1.fastq.gz

