if test -f Fetcher/SRS024301/contents/SRR041919.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041919/SRR041919.fastq.gz -o Fetcher/SRS024301/wget-logs/SRR041919.fastq.gz

if test $(md5sum -c Fetcher/SRS024301/checksums/SRR041919.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041919.fastq.gz Fetcher/SRS024301/contents
fi

fi
