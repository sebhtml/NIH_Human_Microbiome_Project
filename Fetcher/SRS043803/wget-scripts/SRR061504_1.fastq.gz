if test -f Fetcher/SRS043803/contents/SRR061504_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061504/SRR061504_1.fastq.gz -o Fetcher/SRS043803/wget-logs/SRR061504_1.fastq.gz

