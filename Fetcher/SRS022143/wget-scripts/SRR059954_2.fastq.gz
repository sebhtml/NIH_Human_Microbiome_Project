if test -f Fetcher/SRS022143/contents/SRR059954_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059954/SRR059954_2.fastq.gz -o Fetcher/SRS022143/wget-logs/SRR059954_2.fastq.gz

if test $(md5sum -c Fetcher/SRS022143/checksums/SRR059954_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059954_2.fastq.gz Fetcher/SRS022143/contents
