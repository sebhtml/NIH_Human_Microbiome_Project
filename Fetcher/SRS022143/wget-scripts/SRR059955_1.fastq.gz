if test -f Fetcher/SRS022143/contents/SRR059955_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059955/SRR059955_1.fastq.gz -o Fetcher/SRS022143/wget-logs/SRR059955_1.fastq.gz

if test $(md5sum -c Fetcher/SRS022143/checksums/SRR059955_1.fastq.gz|grep OK$|wc -l) = 1
