if test -f Fetcher/SRS024598/contents/SRR040790.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR040/SRR040790/SRR040790.fastq.gz -o Fetcher/SRS024598/wget-logs/SRR040790.fastq.gz

if test $(md5sum -c Fetcher/SRS024598/checksums/SRR040790.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR040790.fastq.gz Fetcher/SRS024598/contents
fi

fi
