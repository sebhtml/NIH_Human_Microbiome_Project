if test -f Fetcher/SRS016600/contents/SRR061981_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061981/SRR061981_1.fastq.gz -o Fetcher/SRS016600/wget-logs/SRR061981_1.fastq.gz

if test $(md5sum -c Fetcher/SRS016600/checksums/SRR061981_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR061981_1.fastq.gz Fetcher/SRS016600/contents
