if test -f Fetcher/SRS016600/contents/SRR041343.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041343/SRR041343.fastq.gz -o Fetcher/SRS016600/wget-logs/SRR041343.fastq.gz

if test $(md5sum -c Fetcher/SRS016600/checksums/SRR041343.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041343.fastq.gz Fetcher/SRS016600/contents
fi

fi
