if test -f Fetcher/SRS016225/contents/SRR061242_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061242/SRR061242_1.fastq.gz -o Fetcher/SRS016225/wget-logs/SRR061242_1.fastq.gz

