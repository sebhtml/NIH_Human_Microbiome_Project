if test -f Fetcher/SRS016225/contents/SRR044381.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR044/SRR044381/SRR044381.fastq.gz -o Fetcher/SRS016225/wget-logs/SRR044381.fastq.gz

if test $(md5sum -c Fetcher/SRS016225/checksums/SRR044381.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR044381.fastq.gz Fetcher/SRS016225/contents
fi

fi
