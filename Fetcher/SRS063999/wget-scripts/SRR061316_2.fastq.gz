	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR061/SRR061316/SRR061316_2.fastq.gz -o Fetcher/SRS063999/wget-logs/SRR061316_2.fastq.gz

