if test -f Fetcher/SRS024435/contents/SRR059342_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059342/SRR059342_2.fastq.gz -o Fetcher/SRS024435/wget-logs/SRR059342_2.fastq.gz

if test $(md5sum -c Fetcher/SRS024435/checksums/SRR059342_2.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR059342_2.fastq.gz Fetcher/SRS024435/contents
fi

fi
