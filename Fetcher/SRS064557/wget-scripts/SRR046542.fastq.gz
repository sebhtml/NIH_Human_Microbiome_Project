if test -f Fetcher/SRS064557/contents/SRR046542.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR046/SRR046542/SRR046542.fastq.gz -o Fetcher/SRS064557/wget-logs/SRR046542.fastq.gz

if test $(md5sum -c Fetcher/SRS064557/checksums/SRR046542.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR046542.fastq.gz Fetcher/SRS064557/contents
fi

fi
