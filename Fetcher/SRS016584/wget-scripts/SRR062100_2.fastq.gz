if test -f Fetcher/SRS016584/contents/SRR062100_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062100/SRR062100_2.fastq.gz -o Fetcher/SRS016584/wget-logs/SRR062100_2.fastq.gz

