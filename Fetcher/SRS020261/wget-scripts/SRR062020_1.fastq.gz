if test -f Fetcher/SRS020261/contents/SRR062020_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062020/SRR062020_1.fastq.gz -o Fetcher/SRS020261/wget-logs/SRR062020_1.fastq.gz

if test $(md5sum -c Fetcher/SRS020261/checksums/SRR062020_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062020_1.fastq.gz Fetcher/SRS020261/contents
fi

fi
