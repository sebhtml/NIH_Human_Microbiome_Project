if test -f Fetcher/SRS014613/contents/SRR045715.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR045/SRR045715/SRR045715.fastq.gz -o Fetcher/SRS014613/wget-logs/SRR045715.fastq.gz

if test $(md5sum -c Fetcher/SRS014613/checksums/SRR045715.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR045715.fastq.gz Fetcher/SRS014613/contents
fi

fi
