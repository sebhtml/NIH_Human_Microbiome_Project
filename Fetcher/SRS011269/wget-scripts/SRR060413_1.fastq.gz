if test -f Fetcher/SRS011269/contents/SRR060413_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060413/SRR060413_1.fastq.gz -o Fetcher/SRS011269/wget-logs/SRR060413_1.fastq.gz

if test $(md5sum -c Fetcher/SRS011269/checksums/SRR060413_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060413_1.fastq.gz Fetcher/SRS011269/contents
fi

fi
