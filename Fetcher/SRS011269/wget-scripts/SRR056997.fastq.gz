if test -f Fetcher/SRS011269/contents/SRR056997.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR056/SRR056997/SRR056997.fastq.gz -o Fetcher/SRS011269/wget-logs/SRR056997.fastq.gz

if test $(md5sum -c Fetcher/SRS011269/checksums/SRR056997.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR056997.fastq.gz Fetcher/SRS011269/contents
fi

fi
