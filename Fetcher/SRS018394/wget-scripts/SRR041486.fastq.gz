if test -f Fetcher/SRS018394/contents/SRR041486.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041486/SRR041486.fastq.gz -o Fetcher/SRS018394/wget-logs/SRR041486.fastq.gz

if test $(md5sum -c Fetcher/SRS018394/checksums/SRR041486.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041486.fastq.gz Fetcher/SRS018394/contents
fi

fi
