if test -f Fetcher/SRS018394/contents/SRR062001_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR062/SRR062001/SRR062001_1.fastq.gz -o Fetcher/SRS018394/wget-logs/SRR062001_1.fastq.gz

if test $(md5sum -c Fetcher/SRS018394/checksums/SRR062001_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR062001_1.fastq.gz Fetcher/SRS018394/contents
fi

fi
