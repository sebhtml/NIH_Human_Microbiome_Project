if test -f Fetcher/SRS013542/contents/SRR059817_2.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR059/SRR059817/SRR059817_2.fastq.gz -o Fetcher/SRS013542/wget-logs/SRR059817_2.fastq.gz

