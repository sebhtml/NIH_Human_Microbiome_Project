if test -f Fetcher/SRS017088/contents/SRR041225.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR041/SRR041225/SRR041225.fastq.gz -o Fetcher/SRS017088/wget-logs/SRR041225.fastq.gz

if test $(md5sum -c Fetcher/SRS017088/checksums/SRR041225.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR041225.fastq.gz Fetcher/SRS017088/contents
fi

fi
