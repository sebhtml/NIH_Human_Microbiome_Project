if test -f Fetcher/SRS065099/contents/SRR060456_1.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR060/SRR060456/SRR060456_1.fastq.gz -o Fetcher/SRS065099/wget-logs/SRR060456_1.fastq.gz

if test $(md5sum -c Fetcher/SRS065099/checksums/SRR060456_1.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR060456_1.fastq.gz Fetcher/SRS065099/contents
fi

fi
