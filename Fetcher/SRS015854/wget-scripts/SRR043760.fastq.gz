if test -f Fetcher/SRS015854/contents/SRR043760.fastq.gz 
then
	echo a > /dev/null
else

wget ftp://ftp.sra.ebi.ac.uk/vol1/fastq/SRR043/SRR043760/SRR043760.fastq.gz -o Fetcher/SRS015854/wget-logs/SRR043760.fastq.gz

if test $(md5sum -c Fetcher/SRS015854/checksums/SRR043760.fastq.gz|grep OK$|wc -l) = 1
then
	mv SRR043760.fastq.gz Fetcher/SRS015854/contents
fi

fi
